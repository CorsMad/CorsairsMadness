function scr_txt_ending_sam1(){
    switch(global.language){
        case 0:
            #region eng
            text1 = "[fnt_pixel]Looks like I'm really gonna have to stay here. I'm no captain without a crew.";
            text2 = "[fnt_pixel]I hope I can be of some use here. Maybe we can find out who this merchant was ...";
            #endregion
            break;
		case 1: //rus
			text1 = "[fnt_pixel_ru]Кажется, и вправду придется остаться здесь. Какой же я капитан без команды...";
			text2 = "[fnt_pixel_ru]Надеюсь, я смогу пригодиться тут. Возможно мы сможем разузнать кем же был этот ... Торговец";
			break;
		case 2: //es
			text1 = "[fnt_pixel]Parece que realmente voy a tener que quedarme aquí. No soy capitán sin una tripulación.";
			text2 = "[fnt_pixel]Espero poder ser de alguna utilidad aquí. Tal vez podamos descubrir quién era este comerciante...";
			break;
		case 3: //cn
			text1 = "[fnt_pixel]看来我真的得留在这里了。没有船员，我就不再是船长。";
			text2 = "[fnt_pixel]我希望我能给这里带来一些帮助。也许我们可以查出那个商人到底是谁...";
			break;
		case 4: //pt
			text1 = "[fnt_pixel]Parece que eu realmente vou ter que ficar aqui. Eu não sou capitão sem uma tripulação.";
			text2 = "[fnt_pixel]Eu espero que eu possa ser útil aqui. Talvez possamos descobrir quem era esse comerciante ...";
			break;
		case 5: //br
			text1 = "[fnt_pixel]Parece que vou ter que ficar mesmo aqui. Não sou capitão sem uma tripulação. ";
			text2 = "[fnt_pixel]Espero poder ser útil aqui. Talvez possamos descobrir quem era este comerciante ...";
			break;
		case 6: //pl
			text1 = "[fnt_pixel]Wygląda na to, że naprawdę będę musiał tu zostać. Bez załogi nie jestem kapitanem.";
			text2 = "[fnt_pixel]Mam nadzieję, że mogę tu być do jakiejś pożytku. Może uda nam się dowiedzieć, kim był ten handlarz ...";
			break;
		case 7: //jp
			text1 = "[fnt_pixel]うも本当にここに残らなければならないようだ。クルーがいなければ私は船長ではない。";
			text2 = "[fnt_pixel]ここで何か役に立てることがあればいいのだが。たぶんこの商人が誰だったのかを見つけ出すことができるかもしれない…。";
			break;
		case 8: //de
			text1 = "[fnt_pixel]Sieht so aus, als müsste ich wirklich hier bleiben. Ich bin kein Kapitän ohne eine Mannschaft.";
			text2 = "[fnt_pixel]Ich hoffe, ich kann hier von Nutzen sein. Vielleicht können wir herausfinden, wer dieser Händler war ...";
			break;
		case 9: //fr
			text1 = "[fnt_pixel]On dirait que je vais vraiment devoir rester ici. Je ne suis pas capitaine sans équipage. ";
			text2 = "[fnt_pixel]J'espère pouvoir être utile ici. Peut-être que nous pouvons découvrir qui était ce marchand ...";
			break;
		case 10: //it
			text1 = "[fnt_pixel]Sembra che dovrò davvero restare qui. Non sono un capitano senza un equipaggio.";
			text2 = "[fnt_pixel]Spero di poter essere di qualche utilità qui. Forse possiamo scoprire chi era questo mercante ...";
			break;
    }
}

