function scr_txt_desert_comt1(){
	switch(global.language)
    {
        case 0: //English  
            #region  ENG text
            text1 = "[fnt_pixel]Oh, [wave]hey[/wave]! I was hoping I'd run into you here. Imagine, in one of the underground buildings, I found this [c_red]ring[/c].";
            text2 = "[fnt_pixel]Look, I snap my fingers and a spirit appears! [wave]How exciting[/wave]!";
            text3 = "[fnt_pixel]I used my gadgets to set it to shoot. Would you like to try it out? I've just prepared an obstacle course.";
            #endregion
            break;
        case 1: //russian
            text1 = "[fnt_pixel_ru]О, [wave]привет[/wave]! Я надеялся встретить тебя тут. Представляешь, в одном из подземных строений я нашел это [c_red]кольцо[/c].";
            text2 = "[fnt_pixel_ru]Смотри, я щелкаю пальцам и появляется дух! [wave]Как захватывающе[/wave]!";
            text3 = "[fnt_pixel_ru]С помощью своих приспособлений я настроил его на выстрел. Не хочешь испытать? Я как раз приготовил полосу препятствий.";
            break;
        case 2: //spanish
            text1 = "[fnt_pixel]¡Oh, [wave]hola[/wave]! Esperaba encontrarte aquí. Imagina, en uno de los edificios subterráneos, encontré este [c_red]anillo[/c].";
			text2 = "[fnt_pixel]Mira, chasqueo mis dedos y aparece un espíritu. ¡[wave]Qué emocionante[/wave]!";
			text3 = "[fnt_pixel]Usé mis artefactos para configurarlo para disparar. ¿Te gustaría probarlo? Acabo de preparar una pista de obstáculos.";
            break;
        case 3: //chinese
            text1 = "[fnt_pixel]哦，[wave]嘿[/wave]！我希望我能在这里碰到你。想象一下，在一个地下建筑里，[c_red]我找到了这个环[/c]。";
			text2 = "[fnt_pixel]看，我一打响指就会出现一个精灵！[wave]多么让人兴奋[/wave]！";
			text3 = "[fnt_pixel]我用我的玩意儿把它设置为射击。你想试一试吗？我刚刚准备了一个障碍赛道。";
            break;
        case 4: //portug
            text1 = "[fnt_pixel]Oh, [wave]oi[/wave]! Eu estava esperando encontrar você aqui. Imagine, em um dos edifícios subterrâneos, [c_red]encontrei este anel[/c].";
			text2 = "[fnt_pixel]Olha, estalo meus dedos e um espírito aparece! [wave]Que emocionante[/wave]!";
			text3 = "[fnt_pixel]Usei meus gadgets para configurá-lo para atirar. Gostaria de experimentar? Acabei de preparar um circuito de obstáculos.";
            break;
        case 5: //brazil
            text1 = "[fnt_pixel]Oh, [wave]oi[/wave]! Eu estava esperando te encontrar aqui. Imagina, em um dos edifícios subterrâneos, eu [c_red]encontrei este anel[/c].";
			text2 = "[fnt_pixel]Olha, eu estalo os dedos e um espírito aparece! [wave]Que excitante[/wave]!";
			text3 = "[fnt_pixel]Usei meus aparelhos para ajustá-lo para atirar. Você gostaria de experimentar? Acabei de preparar um campo de obstáculos.";
            break;
        case 6: //polish
            text1 = "[fnt_pixel]O, [wave]cześć[/wave]! Miałem nadzieję, że spotkam cię tutaj. Wyobraź sobie, w jednym z podziemnych budynków, znalazłem ten [c_red]pierścień[/c].";
			text2 = "[fnt_pixel]Patrz, wystarczy, że pstryknę palcami, a duch się pojawia! [wave]Jak ekscytujące[/wave]!";
			text3 = "[fnt_pixel]Użyłem moich gadżetów, żeby ustawić go na strzelanie. Chciałbyś to wypróbować? Właśnie przygotowałem tor przeszkód.";
            break;
        case 7: //japanese
            text1 = "[fnt_pixel]おお、[wave]こんにちは[/wave]！ ここであなたに会えることを期待していました。 想像してみてください、地下建造物の1つで、[c_red]このリングを見つけました[/c]。";
			text2 = "[fnt_pixel]見て、指を鳴らすと精霊が現れる！ [wave]ワクワクしますね[/wave]！";
			text3 = "[fnt_pixel]私のガジェットを使ってショットするように設定しました。 それを試してみたいですか？ ちょうど障害物コースを準備しました。";
            break;
        case 8: //deutch
            text1 = "[fnt_pixel]Oh, [wave]hey[/wave]! Ich hatte gehofft, dich hier zu treffen. Stell dir vor, in einem der unterirdischen Gebäude habe ich diesen [c_red]Ring[/c] gefunden. ";
			text2 = "[fnt_pixel]Schau mal, ich schnipse mit den Fingern und ein Geist erscheint! [wave]Wie aufregend[/wave]! ";
			text3 = "[fnt_pixel]Ich habe meine Gerätschaften so eingestellt, dass sie schießen. Möchtest du es ausprobieren? Ich habe gerade einen Hindernisparcours vorbereitet.";
            break;
        case 9: //french
            text1 = "[fnt_pixel]Oh, [wave]salut[/wave]! J'espérais te tomber dessus ici. Imagine, dans l'un des bâtiments souterrains, j'ai trouvé cette [c_red]bague[/c].";
			text2 = "[fnt_pixel]Regarde, je claque des doigts et un esprit apparaît! [wave]Que c'est excitant[/wave]!";
			text3 = "[fnt_pixel]J'ai utilisé mes gadgets pour le régler sur tir. Tu aimerais l'essayer? Je viens de préparer un parcours d'obstacles.";
            break;
        case 10: //Italian
            text1 = "[fnt_pixel]Oh, [wave]ciao[/wave]! Speravo di incontrarti qui. Immagina, in uno degli edifici sotterranei, ho trovato questo [c_red]anello[/c].";
			text2 = "[fnt_pixel]Guarda, schiocco le dita e appare uno spirito! [wave]Che emozione[/wave]!";
			text3 = "[fnt_pixel]Ho usato i miei gadget per impostarlo a sparare. Ti piacerebbe provarlo? Ho appena preparato un percorso ad ostacoli.";
            break;
    }    
}
	
function scr_txt_desert_comt2(){
	switch(global.language)
    {
        case 0: //English  
            #region  ENG text
            text1 = "[fnt_pixel]It's pretty simple. Just press the [c_red]spec ability button[/c] and the spirit that appears will release a magic arrow.";
            text2 = "[fnt_pixel]Even though it doesn't deal damage, you should still try it out on the local monsters."; 
			text3 = "[fnt_pixel]You can also break the magical green threads that hold the platforms or knock down targets."
            #endregion
            break;
        case 1: //russian
            text1 = "[fnt_pixel_ru]Все довольно просто. Просто нажми кнопку [c_red]спец. способности[/c] и появившийся дух выпустит магическую стрелу.";
            text2 = "[fnt_pixel_ru]Она хоть и не наносит урон, но тебе все равно стоит опробовать ее на местных монстрах.";
            text3 = "[fnt_pixel_ru]Еще можно разрывать магические зеленые нити, которые держат платформы или сбивать мишени.";
            break;
        case 2: //spanish
            text1 = "[fnt_pixel]Es bastante simple. Solo presiona [c_red]el botón de habilidad especial[/c] y el espíritu que aparece lanzará una flecha mágica.";
			text2 = "[fnt_pixel]Aunque no causa daño, aún deberías probarlo en los monstruos locales.";
			text3 = "[fnt_pixel]También puedes romper los hilos verdes mágicos que sostienen las plataformas o derribar objetivos.";          
            break;
        case 3: //chinese
            text1 = "[fnt_pixel]这很简单。[c_red]只要按下特殊能力按钮[/c]，出现的精灵就会释放一支魔法箭。";
			text2 = "[fnt_pixel]尽管它没有造成伤害，你仍然应该试着在当地的怪物上试一试。";
			text3 = "[fnt_pixel]你还可以打破那些绑在平台上的魔法绿线，或者击倒目标。";
            break;
        case 4: //portug
            text1 = "[fnt_pixel]É bem simples. Basta pressionar o [c_red]botão de habilidade especial[/c] e o espírito que aparece liberará uma flecha mágica.";
			text2 = "[fnt_pixel]Embora não cause danos, você ainda deve testá-lo nos monstros locais.";
			text3 = "[fnt_pixel]Você também pode quebrar os fios verdes mágicos que seguram as plataformas ou derrubar alvos";
            break;
        case 5: //brazil
            text1 = "[fnt_pixel]É bem simples. Basta pressionar o [c_red]botão de habilidade especial[/c] e o espírito que aparece lançará uma flecha mágica.";
			text2 = "[fnt_pixel]Mesmo que não cause dano, você ainda deve experimentá-lo nos monstros locais.";
			text3 = "[fnt_pixel]Você também pode quebrar os fios verdes mágicos que seguram as plataformas ou derrubar alvos.";
            break;
        case 6: //polish
            text1 = "[fnt_pixel]To dość proste. Wystarczy nacisnąć [c_red]przycisk specjalnej umiejętności[/c], a pojawiający się duch wystrzeli magiczną strzałę.";
			text2 = "[fnt_pixel]Chociaż nie zadaje ona obrażeń, powinieneś spróbować jej na lokalnych potworach.";
			text3 = "[fnt_pixel]Możesz też przeciąć magiczne zielone nici, które trzymają platformy, lub strącić cele";
            break;
        case 7: //japanese
            text1 = "[fnt_pixel]それは非常に簡単です。 [c_red]特殊能力ボタンを押すと[/c]、現れる精霊が魔法の矢を放ちます。";
			text2 = "[fnt_pixel]ダメージを与えないけれども、地元のモンスターにそれを試してみるべきです。";
			text3 = "[fnt_pixel]プラットフォームを保持する魔法の緑の糸を壊したり、ターゲットを倒したりすることもできます。";
            break;
        case 8: //deutch
            text1 = "[fnt_pixel]Es ist ziemlich einfach. Drücke einfach die [c_red]Spezialfähigkeitstaste[/c] und der erscheinende Geist wird einen magischen Pfeil abfeuern. ";
			text2 = "[fnt_pixel]Obwohl es keinen Schaden anrichtet, solltest du es trotzdem an den örtlichen Monstern ausprobieren. ";
			text3 = "[fnt_pixel]Du kannst auch die magischen grünen Fäden durchbrechen, die die Plattformen halten oder Ziele umwerfen.";
            break;
        case 9: //french
            text1 = "[fnt_pixel]C'est assez simple. Il suffit d'appuyer sur [c_red]le bouton de capacité spéciale[/c] et l'esprit qui apparaît libérera une flèche magique.";
			text2 = "[fnt_pixel]Même si elle ne fait pas de dégâts, tu devrais quand même l'essayer sur les monstres locaux.";
			text3 = "[fnt_pixel]Tu peux aussi briser les fils verts magiques qui soutiennent les plates-formes ou abattre des cibles.";
            break;
        case 10: //Italian
            text1 = "[fnt_pixel]È piuttosto semplice. Basta premere il [c_red]pulsante dell'abilità speciale[/c] e lo spirito che appare rilascerà una freccia magica.";
			text2 = "[fnt_pixel]Anche se non infligge danni, dovresti comunque provarlo sui mostri locali.";
			text3 = "[fnt_pixel]Puoi anche rompere i fili verdi magici che reggono le piattaforme o abbattere i bersagli.";
            break;
    }    
}
	
function scr_txt_desert_comt3(){
