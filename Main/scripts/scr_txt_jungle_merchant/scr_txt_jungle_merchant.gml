            text0 = "[fnt_pixel]Why don't you go to the [c_red]chief[/c] first, and then we'll chat."
            #endregion
            break;
        case 1: //russian
            #region  кг text
            text0 = "[fnt_pixel_ru]Почему бы вам сначала не сходить к [c_red]вождю[/c], а потом мы поболтаем.";
            #endregion           
            break;
        case 2: //spanish
            #region  ENG text
            text0 = "[fnt_pixel]¿Por qué no vas primero con [c_red]el jefe[/c], y luego charlamos?";
            #endregion
            break;
        case 3: //chinese
            #region  ENG text
            text0 = "[fnt_pixel]你先去找[c_red]首领[/c]吧，然后我们再聊。"
            #endregion
            break;
        case 4: //portug
            #region  ENG text
            text0 = "[fnt_pixel]Por que você não vai primeiro ao [c_red]chefe[/c], e então conversaremos."
            #endregion
            break;
        case 5: //brazil
            #region  ENG text
            text0 = "[fnt_pixel]Por que você não vai ao [c_red]chefe[/c] primeiro, e depois conversamos."
            #endregion
            break;
        case 6: //polish
            #region  ENG text
            text0 = "[fnt_pixel]Dlaczego najpierw nie pójdziesz do [c_red]szefa[/c], a potem porozmawiamy."
            #endregion
            break;
        case 7: //japanese
            #region  ENG text
            text0 = "[fnt_pixel]ま[c_red]ず酋長の[/c]ところに行って、その後で私たちがおしゃべりするといいでしょう。"
            #endregion
            break;
        case 8: //deutch
            #region  ENG text
