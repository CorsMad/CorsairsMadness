function scr_txt_jungle_inventor1(){
    switch(global.language){
        case 0:
            #region eng
            text0 = "[fnt_pixel][wave]Oh oh oh oh.... Greetings![/wave] I'm a local inventor. Helping people with my marvelous inventions.";
            text1 = "[fnt_pixel]Did the chief send you to me? Well, I have just the thing for you.";
            text2 = "[fnt_pixel]Look at these [c_red]boots[/c]. They're [shake]fan-tastic[/shake]!";
            text3 = "[fnt_pixel]I can loan them to you, but first you have to pass my test.";
            text4 = "[fnt_pixel]I built an obstacle course nearby. If you pass it, the boots are yours.";
            #endregion
            break;
		case 1:
			#region RU
			text0 = "[fnt_pixel_ru][wave]Ох ох ох... Приветствую![/wave] Я местный изобретатель. Помогаю людям своими великолепными изобретениями. ";
			text1 = "[fnt_pixel_ru]Это вождь послал тебя ко мне? Что ж у меня как раз есть кое что для тебя.";
			text2 = "[fnt_pixel_ru]Только взгляни на эти [c_red]ботинки[/c]. Они [shake]фан-тас-тические[/shake]!";
			text3 = "[fnt_pixel_ru]Я смогу дать тебе их на время, но сначала тебе необходимо пройти мой тест.";
			text4 = "[fnt_pixel_ru]Я построил полосу препятствий неподалеку. Если справишься с ней - ботинки твои.";
			#endregion
			break;
		case 2:
			#region es
			text0 = "[fnt_pixel][wave]Ho-ho-ho .... ¡Saludos![/wave] Soy un inventor local. Ayudo a la gente con mis maravillosas invenciones.";
			text1 = "[fnt_pixel]¿El jefe te envió conmigo? Bueno, tengo justo lo que necesitas.";
			text2 = "[fnt_pixel]Mira estas [c_red]botas[/c]. ¡Son [shake]fan-tásticas[shake/]!";
			text3 = "[fnt_pixel]Puedo prestarte, pero primero tienes que pasar mi prueba.";
			text4 = "[fnt_pixel]Construí un campo de obstáculos cerca. Si lo pasas, las botas son tuyas.";
			#endregion
			break;
		case 3:
			#region cn
			text0 = "[fnt_pixel][wave]哦哦哦 .... 问候！[/wave]我是一个当地的发明家。用我的神奇发明帮助人们。";
			text1 = "[fnt_pixel]首领把你派给我了吗？好吧，我有正适合你的东西。";
			text2 = "[fnt_pixel]看看这些[c_red]靴子[/c]。它们粉-[shake]棒极了[/shake]！";
			text3 = "[fnt_pixel]我可以把它们借给你，但首先你要通过我的测试。";
			text4 = "[fnt_pixel]我在附近建了一个障碍跑道。如果你通过了，这双靴子就是你的。";
			#endregion
			break;
		case 4:
			#region pt
			text0 = "[fnt_pixel][wave]Ho-ho-ho .... Saudações![/wave] Eu sou o inventor local. Ajudando as pessoas com minhas invenções maravilhosas.";
			text1 = "[fnt_pixel]O chefe te mandou para mim? Bem, eu tenho exatamente o que você precisa.";
			text2 = "[fnt_pixel]Olhe para estas [c_red]botas[/c]. Elas são [shake]fan-tásticas[/shake]!";
			text3 = "[fnt_pixel]Posso emprestá-las a você, mas primeiro você tem que passar no meu teste.";
			text4 = "[fnt_pixel]Eu construí uma pista de obstáculos por perto. Se você passar, as botas são suas.";
			#endregion
			break;
		case 5:
			#region br
			text0 = "[fnt_pixel][wave]Ho-ho-ho .... Saudações![/wave] Sou um inventor local. Ajudando as pessoas com as minhas maravilhosas invenções.";
			text1 = "[fnt_pixel]O chefe te mandou para mim? Bem, eu tenho justamente a coisa para você.";
			text2 = "[fnt_pixel]Olhe para estas [c_red]botas[/c]. Elas são [shake]fan-tásticas[/shake]!";
			text3 = "[fnt_pixel]Eu posso emprestá-las para você, mas primeiro você tem que passar no meu teste.";
			text4 = "[fnt_pixel]Eu construí um campo de obstáculos por perto. Se você passar, as botas são suas.";
			#endregion
			break;
		case 6:
			#region pl
			text0 = "[fnt_pixel][wave]Ho-ho-ho.... Witam![/wave] Jestem lokalnym wynalazcą. Pomagam ludziom moimi zdumiewającymi wynalazkami.";
			text1 = "[fnt_pixel]Czy to wódz wysłał Cię do mnie? Cóż, mam coś dla ciebie.";
			text2 = "[fnt_pixel]Spójrz na te [c_red]buty[/c]. Są [shake]fan-tastyczne[/shake]!";
			text3 = "[fnt_pixel]Mogę Ci je pożyczyć, ale najpierw musisz zdać mój test.";
			text4 = "[fnt_pixel]Zbudowałem niedaleko tor przeszkód. Jeżeli go pokonasz, buty są twoje.";
			#endregion
			break;
		case 7:
			#region jp
			text0 = "[fnt_pixel][wave]ホ-ホ-ホ .... ご挨拶![/wave] 私は地元の発明家です. 素晴らしい発明で人々を助けています.";
			text1 = "[fnt_pixel]酋長はあなたを私に送ったのですか? それならば、ちょうどあなたに向いているものを持っています.";
			text2 = "[fnt_pixel]これらの[c_red]ブーツ[/c]を見てください. それらは[shake]ファン-タスティックです[/shake]!";
			text3 = "[fnt_pixel]私はそれらをあなたに貸すことができます、しかし最初に私の試験に合格する必要があります.";
			text4 = "[fnt_pixel]私は近くに障害物コースを建設しました. あなたがそれをパスすれば、ブーツはあなたのものです.";
			#endregion
			break;
		case 8:
			#region de
			text0 = "[fnt_pixel][wave]Ho-ho-ho .... Grüße![/wave] Ich bin ein örtlicher Erfinder. Hilft den Menschen mit meinen wunderbaren Erfindungen.";
			text1 = "[fnt_pixel]Hat der Häuptling Sie zu mir geschickt? Nun, ich habe genau das Richtige für Sie.";
			text2 = "[fnt_pixel]Sehen Sie sich diese [c_red]Stiefel[/c] an. Sie sind [shake]fan-tastisch[/shake]!";
			text3 = "[fnt_pixel]Ich kann sie Ihnen leihen, aber zuerst müssen Sie meinen Test bestehen.";
			text4 = "[fnt_pixel]Ich habe in der Nähe einen Hindernisparcours gebaut. Wenn Sie ihn bestehen, gehören die Stiefel Ihnen.";
			#endregion
			break;
		case 9:
			#region fr
			text0 = "[fnt_pixel][wave]Ho-ho-ho .... Salutations ![/wave] Je suis un inventeur local. J'aide les gens avec mes merveilleuses inventions.";
			text1 = "[fnt_pixel]Le chef vous a-t-il envoyé à moi ? Eh bien, j'ai juste ce qu'il vous faut.";
			text2 = "[fnt_pixel]Regardez ces [c_red]bottes[/c]. Elles sont [shake]fan-tastiques[/shake] !";
			text3 = "[fnt_pixel]Je peux vous les prêter, mais d'abord, vous devez passer mon test.";
			text4 = "[fnt_pixel]J'ai construit un parcours d'obstacles à proximité. Si vous le passez, les bottes sont à vous.";
			#endregion
			break;
		case 10:
			#region it
			text0 = "[fnt_pixel][wave]Ho-ho-ho .... Saluti![/wave] Sono un inventore locale. Aiuto la gente con le mie meravigliose invenzioni.";
			text1 = "[fnt_pixel]Il capo ti ha mandato da me? Beh, ho proprio la cosa giusta per te.";
			text2 = "[fnt_pixel]Guarda queste [c_red]stivali[/c]. Sono [shake]fan-tastici[/shake]!";
			text3 = "[fnt_pixel]Posso prestarteli, ma prima devi superare il mio test.";
			text4 = "[fnt_pixel]Ho costruito un percorso ad ostacoli nelle vicinanze. Se lo superi, gli stivali sono tuoi.";
			#endregion
			break;
    }
}

function scr_txt_jungle_inventor2(){
    switch(global.language){
        case 0:
            #region eng
            text0 = "[fnt_pixel]So, in order to use the spring boots, you have to press the [c_red]jump button[/c] in the air.";
            text1 = "[fnt_pixel]Just like that! On a normal surface you will not jump away, but from the spikes - easily!";
            text2 = "[fnt_pixel]Maybe the local enemies can be affected somehow.";
