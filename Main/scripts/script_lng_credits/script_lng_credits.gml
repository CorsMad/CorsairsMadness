function script_lng_after_credits(){
	switch(global.language){
		case 0:
			#region Eng
			text1 = "[fnt_pixel]Captain, what's next? Where do we go from here?";
			text2 = "[fnt_pixel]I don't care anymore. I finally want to relax in some cheap tavern after all we've been through...";
			text3 = "[fnt_pixel]I miss our new friends already.";
			text4 = "[fnt_pixel]I don't think we're gonna be apart for long, Jakey.";
			text5 = "[fnt_pixel]Yeah! I can't wait to work out with the inventor! Notice how big his muscle is?";
			text6 = "[fnt_pixel]Speaking of which. Is it just me, or does the inventor look kind of weird?";
			text7 = "[fnt_pixel]Yeah, that's what it looked like to me, too. Like he wasn't himself.";
			text8 = "[fnt_pixel]My heart tells me we're not done with this archipelago yet.";
			#endregion
			break;
		case 1: //rus
			text1 = "[fnt_pixel_ru]Капитан, что дальше? Куда направимся?";
			text2 = "[fnt_pixel_ru]Мне уже все равно. Я наконец-то хочу расслабиться в какой-нибудь дешевой таверне после всего, что мы пережили...";
			text3 = "[fnt_pixel_ru]Я уже скучаю по нашим новым друзьям.";
			text4 = "[fnt_pixel_ru]Не думаю, что мы расстанемся надолго, Джейки.";
			text5 = "[fnt_pixel_ru]Да! Не могу дождаться, когда смогу потренироваться с изобретателем! Заметили, какие у него большие мышцы?";
			text6 = "[fnt_pixel_ru]Кстати, об этом. Мне кажется, или изобретатель выглядел как-то странно?";
			text7 = "[fnt_pixel_ru]Да, мне тоже так показалось. Как будто он был не в себе.";
			text8 = "[fnt_pixel_ru]мое Сердце подсказывает мне, что мы еще не закончили с этим архипелагом.";
			break;
		case 2: //es
			text1 = "[fnt_pixel]Capitán, ¿qué sigue? ¿A dónde vamos desde aquí?";
			text2 = "[fnt_pixel]Ya no me importa. Finalmente quiero relajarme en alguna taberna barata después de todo lo que hemos pasado...";
			text3 = "[fnt_pixel]Ya echo de menos a nuestros nuevos amigos.";
			text4 = "[fnt_pixel]No creo que vayamos a estar separados mucho tiempo, Jakey.";
			text5 = "[fnt_pixel]¡Sí! ¡No puedo esperar a entrenar con el inventor! ¿Notas lo grande que es su músculo?";
			text6 = "[fnt_pixel]Hablando de eso. ¿Soy yo, o el inventor se ve un poco raro?";
			text7 = "[fnt_pixel]Sí, eso es lo que me pareció a mí también. Como si no fuera él mismo.";
			text8 = "[fnt_pixel]Mi corazón me dice que aún no terminamos con este archipiélago.";
			break;
		case 3: //cn
			text1 = "[fnt_pixel]队长，接下来怎么办？我们该何去何从？";
			text2 = "[fnt_pixel]我已经不在乎了 在经历了这么多之后，我终于想在廉价的小酒馆里放松一下了...";
			text3 = "[fnt_pixel]我已经开始想念我们的新朋友了";
			text4 = "[fnt_pixel]我想我们不会分开太久的 Jakey";
			text5 = "[fnt_pixel]是啊 我等不及要和发明家一起锻炼了 注意到他的肌肉有多大了吗";
			text6 = "[fnt_pixel]说到这个 是我的错觉 还是发明家看起来有点怪？";
			text7 = "[fnt_pixel]是啊，我也这么觉得 好像他不是他自己";
			text8 = "[fnt_pixel]我的心告诉我 这个群岛还没完呢";
			break;
		case 4: //pt
			text1 = "[fnt_pixel]Capitão, o que é que se segue? Para onde é que vamos a partir daqui?";
			text2 = "[fnt_pixel]Já não me interessa. Finalmente quero relaxar numa taberna barata depois de tudo o que passámos...";
			text3 = "[fnt_pixel]Já sinto falta dos nossos novos amigos.";
			text4 = "[fnt_pixel]Acho que não nos vamos separar por muito tempo, Jakey.";
			text5 = "[fnt_pixel]Boa! Mal posso esperar para treinar com o inventor! Já viste como os músculos dele são grandes?";
			text6 = "[fnt_pixel]Por falar nisso. É impressão minha, ou o inventor parece um bocado esquisito?";
			text7 = "[fnt_pixel]Sim, foi o que me pareceu também. Como se ele não estivesse em si.";
			text8 = "[fnt_pixel]O meu coração diz-me que ainda não acabámos com este arquipélago.";
			break;
		case 5: //br
			text1 = "[fnt_pixel]Capitão, o que vem a seguir? Para onde vamos a partir daqui?";
			text2 = "[fnt_pixel]Eu não me importo mais. Finalmente quero relaxar em alguma taverna barata depois de tudo o que passamos...";
			text3 = "[fnt_pixel]Já estou sentindo falta de nossos novos amigos.";
			text4 = "[fnt_pixel]Acho que não vamos nos separar por muito tempo, Jakey.";
			text5 = "[fnt_pixel]Sim! Mal posso esperar para treinar com o inventor! Reparou como seus músculos são grandes?";
			text6 = "[fnt_pixel]Falando nisso. É impressão minha ou o inventor parece meio esquisito?";
			text7 = "[fnt_pixel]Sim, foi o que pareceu para mim também. Como se ele não fosse ele mesmo.";
			text8 = "[fnt_pixel]Meu coração me diz que ainda não terminamos com esse arquipélago.";
			break;
		case 6: //pl
			text1 = "[fnt_pixel]Kapitanie, co dalej? Co dalej?";
			text2 = "[fnt_pixel]Już mnie to nie obchodzi. Chcę wreszcie odpocząć w jakiejś taniej tawernie po tym wszystkim, co przeszliśmy...";
			text3 = "[fnt_pixel]Już tęsknię za naszymi nowymi przyjaciółmi.";
			text4 = "[fnt_pixel]Nie sądzę, żebyśmy się długo rozstawali, Jakey.";
			text5 = "[fnt_pixel]Tak! Nie mogę się doczekać treningu z wynalazcą! Zauważyłeś, jak duże ma mięśnie?";
			text6 = "[fnt_pixel]A propos. Czy tylko mi się wydaje, czy wynalazca wygląda jakoś dziwnie?";
			text7 = "[fnt_pixel]Tak, dla mnie też tak to wyglądało. Jakby nie był sobą.";
			text8 = "[fnt_pixel]Moje serce mówi mi, że jeszcze nie skończyliśmy z tym archipelagiem.";
			break;
		case 7: //jp
			text1 = "[fnt_pixel]キャプテン、次はどうするんですか？これからどうするんだ？";
			text2 = "[fnt_pixel]もうどうでもいい。やっと安酒場でくつろぎたくなってきたんだ。";
			text3 = "[fnt_pixel]もう新しい友達が恋しいよ。";
			text4 = "[fnt_pixel]長くは離れられないと思うよ、ジェイキー。";
			text5 = "[fnt_pixel]そうだね！発明家とトレーニングするのが待ちきれないよ！彼の筋肉の大きさに気づいた？";
			text6 = "[fnt_pixel]そういえば。発明家が変に見えるのは俺だけか？";
			text7 = "[fnt_pixel]ああ、僕にもそう見えたよ。自分じゃないみたいな";
			text8 = "[fnt_pixel]この列島はまだ終わっていない、と私の心は告げている。";
			break;
		case 8: //de
			text1 = "[fnt_pixel]Kapitän, wie geht es weiter? Wie geht es jetzt weiter?";
			text2 = "[fnt_pixel]Das interessiert mich nicht mehr. Ich will mich endlich in einer billigen Taverne entspannen, nach allem, was wir durchgemacht haben...";
			text3 = "[fnt_pixel]Ich vermisse unsere neuen Freunde jetzt schon.";
			text4 = "[fnt_pixel]Ich glaube nicht, dass wir lange getrennt sein werden, Jakey.";
			text5 = "[fnt_pixel]Ich kann es kaum erwarten zu trainieren. Ich kann es nicht erwarten, mit dem Erfinder zu trainieren! Hast du bemerkt, wie groß seine Muskeln sind?";
			text6 = "[fnt_pixel]Wo wir gerade dabei sind. Geht es nur mir so, oder sieht der Erfinder irgendwie komisch aus?";
			text7 = "[fnt_pixel]Ja, so sah es für mich auch aus. Als ob er nicht er selbst wäre.";
			text8 = "[fnt_pixel]Mein Herz sagt mir, dass wir mit diesem Archipel noch nicht fertig sind.";
			break;
		case 9: //fr
			text1 = "[fnt_pixel]Capitaine, quelle est la suite ? Où allons-nous maintenant ?";
			text2 = "[fnt_pixel]Je n'en ai plus rien à faire. J'ai enfin envie de me détendre dans une taverne bon marché après tout ce que nous avons vécu...";
			text3 = "[fnt_pixel]Nos nouveaux amis me manquent déjà.";
			text4 = "[fnt_pixel]Je ne pense pas que nous serons séparés longtemps, Jakey.";
			text5 = "[fnt_pixel]Ouais ! J'ai hâte de m'entraîner avec l'inventeur ! Tu as remarqué la taille de ses muscles ?";
			text6 = "[fnt_pixel]En parlant de ça. C'est moi, ou l'inventeur a l'air bizarre ?";
			text7 = "[fnt_pixel]Oui, c'est ce que j'ai ressenti aussi. Comme s'il n'était pas lui-même.";
			text8 = "[fnt_pixel]Mon cœur me dit que nous n'en avons pas encore fini avec cet archipel.";
			break;
		case 10: //it
			text1 = "[fnt_pixel]Capitano, qual è il prossimo passo? Dove andremo a finire?";
			text2 = "[fnt_pixel]Non mi interessa più. Voglio finalmente rilassarmi in qualche taverna da quattro soldi dopo tutto quello che abbiamo passato...";
			text3 = "[fnt_pixel]Mi mancano già i nostri nuovi amici.";
			text4 = "[fnt_pixel]Non credo che staremo lontani a lungo, Jakey.";
			text5 = "[fnt_pixel]Sì! Non vedo l'ora di allenarmi con l'inventore! Hai notato quanto sono grossi i suoi muscoli?";
			text6 = "[fnt_pixel]A proposito. Sono io, o l'inventore sembra un po' strano?";
			text7 = "[fnt_pixel]Sì, anche a me è sembrato così. Come se non fosse in sé.";
			text8 = "[fnt_pixel]Il mio cuore mi dice che non abbiamo ancora finito con questo arcipelago.";
			break;
	}
}
