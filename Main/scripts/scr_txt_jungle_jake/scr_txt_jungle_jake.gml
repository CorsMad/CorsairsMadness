function scr_txt_jungle_jake(){
    switch(global.language){
        case 0:
            #region English
            text1 = "[fnt_pixel]Oh Captain, hello! What the hell is going on here?";
            text2 = "[fnt_pixel]As soon as I woke up on this island my feet carried me to this temple.";
            text3 = "[fnt_pixel]Then I saw the mask and I couldn't control myself. Luckily you came to the rescue.";
            text4 = "[fnt_pixel]First a glow.... then a flash... and I'm standing here as good as new!";
            text5 = "[fnt_pixel]I'll stay here for now, and you go get the others.";
            text6 = "[fnt_pixel]Here's a little present from me.";
            #endregion
            break;
    }
}


function scr_txt_jungle_jakefol1(){
    switch(global.language){
        case 0:
            #region eng
            text1 = "[fnt_pixel]Well, hello there. captain.";
            text2 = "[fnt_pixel]And how do you like the power of the mask? Tell me, is it really [shake]amazing[/shake]? ";
            text3 = "[fnt_pixel]Although I was healthy, but now I'm even [shake]stronger[/shake]!";
            text4 = "[fnt_pixel]Come on, try hitting me and see for yourself!";
            #endregion
            break;
    }
}

function scr_txt_jungle_jakefol2(){
    switch(global.language){
        case 0:
            #region eng
            text1 = "[fnt_pixel][shake]Ha ha ha[/shake]! It's useless!";
            text2 = "[fnt_pixel]Your attacks are like a tickle to me.";
            text3 = "[fnt_pixel]And yet, you've come to take away my power. I can't let that happen, you know.";
            text4 = "[fnt_pixel]But I'll give you a bit of a head start, since you used to be my captain.";
            text5 = "[fnt_pixel]Damn, I just love my new power!";
            #endregion
            break;
    }
}

function scr_txt_jungle_jakefol3(){
    switch(global.language){
        case 0:
            #region eng
            text1 = "[fnt_pixel]There you are.";
            text2 = "[fnt_pixel]You think anything's changed since the last time I saw you?";
            text3 = "[fnt_pixel]I'm gonna squash you like a rat!";
            #endregion
