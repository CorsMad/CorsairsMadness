function scr_txt_ending_chief_jungle(){
	switch(global.language){
		case 0:
			#region eng
			text1 = "[fnt_pixel]Good job! Now we can sleep well.";
			text2 = "[fnt_pixel]You even managed to save [c_red]Jake[/c]!";
			#endregion
			break;
	}	
}

