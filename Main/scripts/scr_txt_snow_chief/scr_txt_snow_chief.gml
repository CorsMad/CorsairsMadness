function scr_txt_snow_chief1(){
    switch(global.language){
        case 0:   
            #region English 
            text1 = "[fnt_pixel]Welcome to our [c_aqua]snow island[/c]!";
            text2 = "[fnt_pixel][shake]What[/shake]? Aren't you cold? It's freezing and snowing outside and you're not cold!";
            text3 = "[fnt_pixel]Maybe this cold has no effect on the newcomers.";
            text4 = "[fnt_pixel]I'm saying this because recently there was a [c_red]guy[/c] ...[delay,500] [shake]wearing nothing but his underpants[/shake]!";
            text5 = "[fnt_pixel]Even though it was before his visit, but we're still, you know. it's not a resort.";
            text6 = "[fnt_pixel]And later on, it got even worse! I'm sure it has something to do with that [c_red]man in underpants[/c].";
            text7 = "[fnt_pixel]Since you don't care about the cold, maybe you can figure it out.";
            #endregion
            break;
		case 1:	//rus
			text1 = "[fnt_pixel_ru]Добро пожаловать на наш [c_aqua]снежный остров[/c]!";
			text2 = "[fnt_pixel_ru][shake]ЧТО[/shake]? тебе не холодно? На улице жуткий холод и снегопад, а тебе не холодно!";
			text3 = "[fnt_pixel_ru]Возможно на приезжахи эта стужа и холод никак не влияет.";
			text4 = "[fnt_pixel_ru]Я говорю это потому что недавно тут был [c_red]парень[/c] ... [delay,500][shake]В ОДНИХ ТРУСАХ[/shake]!";
			text5 = "[fnt_pixel_ru]Хоть это было и до его посещения, но у нас все равно тут знаешь ли... не курорт.";
			text6 = "[fnt_pixel_ru]А чуть позже все стало еще хуже! Я уверен что это как то связано с этим [c_red]человеком в трусах[/c]!";
			text7 = "[fnt_pixel_ru]Раз уж тебе холод нипочем может быть ты сможешь разобраться с этим?";
			break;
		case 2: //es
			text1 = "[fnt_pixel]¡Bienvenidos a nuestra [c_aqua]isla de nieve[/c]!";
			text2 = "[fnt_pixel]¿[shake]Qué[/shake]? ¿No tienes frío? Hace un frío terrible y nieve afuera ¡y tú no tienes frío!";
			text3 = "[fnt_pixel]Quizás este frío no tenga efecto en los recién llegados.";
			text4 = "[fnt_pixel]Digo esto porque recientemente hubo un [c_red]tipo[/c]...[delay,500]¡[shake]sin nada más que sus calzoncillos[/shake]!";
			text5 = "[fnt_pixel]Ni siquiera estaba antes de su visita, pero todavía, ya sabes. No es un centro vacacional.";
			text6 = "[fnt_pixel]¡Y más tarde, empeoró aún más! Estoy seguro de que tiene algo que ver con ese [c_red]hombre en calzoncillos[/c].";
			text7 = "[fnt_pixel]Ya que no te importa el frío, tal vez puedas averiguarlo.";
			break;
		case 3: //cn
			text1 = "[fnt_pixel]欢迎来到我们[c_aqua]的雪岛[/c]！";
			text2 = "[fnt_pixel][shake]什么[/shake]？你不觉得冷吗？外面冻得要命，还在下雪，你却不觉得冷！";
			text3 = "[fnt_pixel]也许这种寒冷对新来的人没有影响。";
			text4 = "[fnt_pixel]我这么说是因为最近有[c_red]个家伙[/c]...[delay,500][shake]他身上什么都没穿，只穿了一条内裤[/shake]！";
			text5 = "[fnt_pixel]即使那是在他来访之前，但我们仍然，你知道的，这不是度假胜地。";
			text6 = "[fnt_pixel]之后，情况变得更糟！我敢肯定那[c_red]个穿着内裤的男人和[/c]这件事有关。";
			text7 = "[fnt_pixel]既然你不在乎寒冷，也许你可以找出原因。";
			break;
		case 4: //pt
			text1 = "[fnt_pixel]Bem-vindo à nossa [c_aqua]ilha de neve[/c]!";
			text2 = "[fnt_pixel][shake]O quê[/shake]? Você não está com frio? Está congelando e nevando lá fora e você não está com frio!";
			text3 = "[fnt_pixel]Talvez este frio não tenha efeito sobre os recém-chegados.";
			text4 = "[fnt_pixel]Estou dizendo isso porque recentemente tinha um [c_red]cara[/c]...[delay,500][shake]vestindo apenas suas cuecas[/shake]!";
			text5 = "[fnt_pixel]Mesmo que tenha sido antes da sua visita, mas ainda estamos, você sabe. não é um resort.";
			text6 = "[fnt_pixel]E mais tarde, ficou ainda pior! Tenho certeza de que tem algo a ver com aquele [c_red]homem de cuecas[/c].";
			text7 = "[fnt_pixel]Como você não se importa com o frio, talvez você possa descobrir.";
			break;
		case 5: //br
			text1 = "[fnt_pixel]Bem-vindo à nossa [c_aqua]ilha de neve[/c]!";
			text2 = "[fnt_pixel][shake]O quê[/shake]? Você não está com frio? Está congelando e nevando lá fora e você não está com frio!";
			text3 = "[fnt_pixel]Talvez esse frio não tenha efeito sobre os recém-chegados.";
			text4 = "[fnt_pixel]Estou dizendo isso porque recentemente havia um [c_red]cara[/c]...[delay,500][shake]usando nada além de suas cuecas[/shake]!";
			text5 = "[fnt_pixel]Mesmo que tenha sido antes de sua visita, mas ainda estamos, sabe. não é um resort.";
			text6 = "[fnt_pixel]E mais tarde, ficou ainda pior! Tenho certeza de que tem algo a ver com aquele [c_red]homem de cuecas[/c].";
			text7 = "[fnt_pixel]Como você não se preocupa com o frio, talvez possa descobrir isso.";
			break;
		case 6: //pl
			text1 = "[fnt_pixel]Witamy na naszej [c_aqua]śnieżnej wyspie[/c]!";
			text2 = "[fnt_pixel][shake]Co[/shake]? Nie jest ci zimno? Na zewnątrz jest mroźno i pada śnieg, a ty nie jesteś zimno!";
			text3 = "[fnt_pixel]Może ten zimno nie ma wpływu na nowoprzybyłych.";
			text4 = "[fnt_pixel]Mówię to, ponieważ ostatnio był u nas [c_red]facet[/c]...[delay,500][shake]ubrany tylko w bieliznę[/shake]!";
			text5 = "[fnt_pixel]Mimo, że to było przed jego wizytą, ale wciąż, wiesz. to nie jest kurort.";
			text6 = "[fnt_pixel]A później było jeszcze gorzej! Jestem pewny, że ma to coś wspólnego z tym [c_red]mężczyzną w bieliźnie[/c].";
			text7 = "[fnt_pixel]Skoro nie zależy ci na zimnie, może ty będziesz w stanie to rozgryźć.";
			break;
		case 7: //jp
			text1 = "[fnt_pixel]私たちの[c_aqua]雪の島[/c]へようこそ！";
			text2 = "[fnt_pixel][shake]何[/shake]？ 寒くない？ 外は凍えるほどの寒さで雪が降っていて、あなたは寒くない！";
			text3 = "[fnt_pixel]この寒さは新入りには影響しないのかもしれません。";
			text4 = "[fnt_pixel]これを言っているのは、最近、[delay,500][shake]下着だけを着ている男がいたからです[/shake]！";
			text5 = "[fnt_pixel]彼の訪問の前ではありましたが、私たちはまだ、あなたが知っているように、これはリゾートではありません。";
			text6 = "[fnt_pixel]そしてその後、さらにひどくなりました！ 私はそれが[c_red]下着の男[/c]と何か関係があると確信しています。";
			text7 = "[fnt_pixel]寒さを気にしないのなら、あなたがそれを理解することができるかもしれません。";
			break;
		case 8: //de
			text1 = "[fnt_pixel]Willkommen auf unserer [c_aqua]Schneeinsel[/c]! ";
			text2 = "[fnt_pixel][shake]Was[/shake]? Ist dir nicht kalt? Es ist eiskalt und es schneit draußen und dir ist nicht kalt! ";
			text3 = "[fnt_pixel]Vielleicht hat diese Kälte keinen Einfluss auf die Neuankömmlinge. ";
			text4 = "[fnt_pixel]Ich sage das, weil es vor kurzem einen [c_red]Kerl[/c] gab...[delay,500][shake]der nichts als seine Unterhose trug[/shake]! ";
			text5 = "[fnt_pixel]Obwohl es vor seinem Besuch war, sind wir immer noch, wissen Sie. das ist kein Ferienort. ";
			text6 = "[fnt_pixel]Und später wurde es noch schlimmer! Ich bin sicher, dass es etwas mit diesem [c_red]Mann in Unterhose[/c] zu tun hat. ";
			text7 = "[fnt_pixel]Da du die Kälte nicht scheust, kannst du vielleicht herausfinden, was los ist.";
			break;
		case 9: //fr
			text1 = "[fnt_pixel]Bienvenue sur notre [c_aqua]île de neige[/c]!";
			text2 = "[fnt_pixel][shake]Quoi[/shake]? Tu n'as pas froid? Il gèle et neige dehors et tu n'as pas froid!";
			text3 = "[fnt_pixel]Peut-être que ce froid n'a pas d'effet sur les nouveaux venus.";
			text4 = "[fnt_pixel]Je dis cela parce que récemment il y avait un [c_red]gars[/c]...[delay,500][shake]portant rien d'autre que ses sous-vêtements[/shake]!";
			text5 = "[fnt_pixel]Même si c'était avant sa visite, mais nous sommes toujours, tu sais. ce n'est pas une station balnéaire.";
			text6 = "[fnt_pixel]Et plus tard, c'est devenu encore pire! Je suis sûr que cela a quelque chose à voir avec cet [c_red]homme en sous-vêtements[/c].";
			text7 = "[fnt_pixel]Puisque tu te fiches du froid, peut-être que tu peux le comprendre.";
			break;
		case 10: //it
			text1 = "[fnt_pixel]Benvenuto nella nuestra [c_aqua]isola nevosa[/c]!";
			text2 = "[fnt_pixel][shake]Cosa[/shake]? Non hai freddo? Fuori è gelido e nevica e tu non hai freddo!";
			text3 = "[fnt_pixel]Forse questo freddo non ha effetto sui nuovi arrivati.";
			text4 = "[fnt_pixel]Dico questo perché recentemente c'era un [c_red]tizio[/c]...[delay,500][shake]che indossava solo le sue mutande[/shake]!";
			text5 = "[fnt_pixel]Anche se era prima della sua visita, ma siamo ancora, sai. non è una località turistica.";
			text6 = "[fnt_pixel]E più tardi, è peggiorato ancora di più! Sono sicuro che ha qualcosa a che fare con [c_red]quell'uomo in mutande[/c].";
			text7 = "[fnt_pixel]Siccome a te non importa del freddo, forse puoi capirlo.";
			break;
    }
}

function scr_txt_snow_chief2(){
    switch(global.language){
        case 0:   
            #region English 
            text1 = "[fnt_pixel]I see you fixed it, huh? The cold is gone with the snowfall.";
            text2 = "[fnt_pixel]And I still can't get [shake]warm[/shake]...maybe I need a hot water bath or a drink of something hot. Heh heh heh heh.";
            text3 = "[fnt_pixel]You did a great job! Here's a [c_yellow]little present[/c] from us.";
            #endregion
            break;
		case 1: //rus
			text1 = "[fnt_pixel_ru]Я смотрю ты все исправил? Холод ушел вместе со снегопадом. ";
			text2 = "[fnt_pixel_ru]А я все никак не могу [shake]согреться[/shake]...может быть нужно попариться в горячей воде или выпить чего-нибудь горячительного хе хе хе.";
			text3 = "[fnt_pixel_ru]А ты отлично справился! Вот тебе [c_yellow]небольшой подарок[/c] от нас.";
			break;
		case 2: //es
			text1 = "[fnt_pixel]Veo que lo arreglaste, ¿eh? El frío se ha ido con la nevada.";
			text2 = "[fnt_pixel]Y todavía no puedo [shake]entrar en calor[/shake]... tal vez necesite un baño de agua caliente o un trago de algo caliente. Je je je je.";
			text3 = "[fnt_pixel]¡Hiciste un gran trabajo! Aquí tienes un [c_yellow]pequeño regalo[/c] de nuestra parte.";
			break;
		case 3: //cn
			text1 = "[fnt_pixel]我看你修好了，是吗？随着雪的降落，寒冷也消失了。";
			text2 = "[fnt_pixel][shake]我还是温暖不起来[/shake]...也许我需要一个热水浴或者喝点热的东西。呵呵呵呵。";
			text3 = "[fnt_pixel]你做得很棒！这是我们的一[c_yellow]点小礼物[/c]。";
			break;
		case 4: //pt
			text1 = "[fnt_pixel]Vejo que você consertou, hein? O frio se foi com a nevasca.";
			text2 = "[fnt_pixel]E ainda não consigo me [shake]aquecer[/shake]... talvez eu precise de um banho de água quente ou de uma bebida quente. Heh heh heh heh.";
			text3 = "[fnt_pixel]Você fez um ótimo trabalho! Aqui está um [c_yellow]pequeno presente[/c] nosso.";
			break;
		case 5: //br
			text1 = "[fnt_pixel]Vejo que você consertou, hein? O frio se foi com a neve.";
			text2 = "[fnt_pixel]E eu ainda não consigo me [shake]aquecer[/shake]... talvez eu precise de um banho de água quente ou de uma bebida quente. Heh heh heh heh.";
			text3 = "[fnt_pixel]Você fez um ótimo trabalho! Aqui está um [c_yellow]pequeno presente[/c] de nós.";
			break;
		case 6: //pl
			text1 = "[fnt_pixel]Widzę, że naprawiłeś to, co? Zimno odeszło razem ze śniegiem.";
			text2 = "[fnt_pixel]A ja nadal nie mogę [shake]się ogrzać[/shake]... może potrzebuję gorącej kąpieli lub czegoś gorącego do picia. Heh heh heh heh.";
			text3 = "[fnt_pixel]Świetnie się spisałeś! Oto [c_yellow]mały prezent[/c] od nas.";
			break;
		case 7: //jp
			text1 = "[fnt_pixel]それを直したのですか、ええ?寒さは雪と共に消え去りました。";
			text2 = "[fnt_pixel][shake]そして私はまだ暖かさを取り戻せない[/shake]...おそらく温かい風呂に入るか、何か温かい飲み物が必要かもしれません。 へへへへ。";
			text3 = "[fnt_pixel]あなたは素晴らしい仕事をしました！ [c_yellow]こちらから小さなプレゼントです[/c]。";
			break;
		case 8: //de
			text1 = "[fnt_pixel]Ich sehe, du hast es repariert, hm? Die Kälte ist mit dem Schneefall weg. ";
			text2 = "[fnt_pixel]Und ich kann immer noch nicht [shake]warm werden[/shake]... vielleicht brauche ich ein heißes Bad oder ein heißes Getränk. Heh heh heh heh. ";
			text3 = "[fnt_pixel]Du hast einen tollen Job gemacht! Hier ist ein [c_yellow]kleines Geschenk[/c] von uns.";
			break;
		case 9: //fr
			text1 = "[fnt_pixel]Je vois que tu as corrigé cela, hein? Le froid est parti avec les chutes de neige.";
			text2 = "[fnt_pixel]Et je n'arrive toujours pas à me [shake]réchauffer[/shake]...peut-être que j'ai besoin d'un bain d'eau chaude ou de boire quelque chose de chaud. Heh heh heh heh.";
			text3 = "[fnt_pixel]Tu as fait du bon travail! Voici un [c_yellow]petit cadeau[/c] de notre part.";
			break;
		case 10: //it
			text1 = "[fnt_pixel]Vedo che l'hai sistemato, eh? Il freddo è andato via con la nevicata.";
			text2 = "[fnt_pixel]E io non riesco ancora a [shake]scaldarmi[/shake]...forse ho bisogno di un bagno d'acqua calda o di una bevanda calda. Eheheheheh.";
			text3 = "[fnt_pixel]Hai fatto un ottimo lavoro! Ecco un [c_yellow]piccolo regalo[/c] da parte nostra.";
			break;
    }
}

