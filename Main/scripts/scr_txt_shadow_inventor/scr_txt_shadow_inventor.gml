			break;
		case 4: // pt
			text1 = "[fnt_pixel]Tudo o que tens de fazer é manter premido o [c_red]botão de habilidade especial[/c].";
			text2 = "[fnt_pixel]Então, depois de um tempo, uma aura musical aparecerá ao seu redor e os objetos começarão a se mover!";
			text3 = "[fnt_pixel]Tente usá-la em inimigos locais e cogumelos. Acho que o efeito vai te surpreender.";
			break;
		case 5: // br
			text1 = "[fnt_pixel]Tudo o que você precisa fazer é manter pressionado o [c_red]botão de habilidade especial[/c].";
			text2 = "[fnt_pixel]Então, depois de um tempo, uma aura musical aparecerá ao seu redor e os objetos começarão a se mover!";
			text3 = "[fnt_pixel]Tente usá-lo nos inimigos locais e cogumelos. Acho que o efeito vai te surpreender.";
			break;
		case 6: // pl
			text1 = "[fnt_pixel]Wszystko, co musisz zrobić, to przytrzymać [c_red]przycisk specjalnej umiejętności[/c].";
			text2 = "[fnt_pixel]Potem po chwili wokół ciebie pojawi się muzyczna aura i przedmioty zaczną się poruszać!";
			text3 = "[fnt_pixel]Spróbuj użyć go na lokalnych wrogach i grzybach. Myślę, że efekt cię zaskoczy.";
			break;
		case 7: // jp
			text1 = "[fnt_pixel]あなたがしなければならないのは、[c_red]特殊能力ボタンを押し続けるだけ[/c]。";
			text2 = "[fnt_pixel]するとしばらくすると音楽のオーラがあなたの周りに現れ、物体が動き始めます！";
			text3 = "[fnt_pixel]地元の敵やキノコにそれを使ってみてください。効果はあなたを驚かせると思います。";
			break;
		case 8: // de
			text1 = "[fnt_pixel]Alles, was Sie tun müssen, ist, die Taste für die [c_red]Spezialfähigkeit[/c] zu drücken und zu halten.";
			text2 = "[fnt_pixel]Dann erscheint nach einer Weile eine musikalische Aura um dich herum und Objekte fangen an sich zu bewegen!";
			text3 = "[fnt_pixel]Versuche es bei lokalen Feinden und Pilzen einzusetzen. Ich denke, der Effekt wird dich überraschen.";
			break;
		case 9: // fr
			text1 = "[fnt_pixel]Il suffit d'appuyer sur le bouton correspondant à la [c_red]capacité spéciale[/c] et de le maintenir enfoncé.";
			text2 = "[fnt_pixel]Ensuite, après un moment, une aura musicale apparaîtra autour de vous et les objets commenceront à se déplacer!";
			text3 = "[fnt_pixel]Essayez de l'utiliser sur les ennemis locaux et les champignons. Je pense que l'effet vous surprendra.";
			break;
		case 10: // it
			text1 = "[fnt_pixel]È sufficiente tenere premuto il pulsante corrispondente [c_red]all'abilità speciale[/c].";
			text2 = "[fnt_pixel]Poi, dopo un po', apparirà un'aura musicale attorno a te e gli oggetti inizieranno a muoversi!";
			text3 = "[fnt_pixel]Prova ad usarlo sui nemici locali e sui funghi. Penso che l'effetto ti sorprenderà.";
			break;
    }
}

