function scr_txt_shadow_inventor1(){
    switch(global.language){
        case 0:
            #region English
            text1 = "[fnt_pixel]Hello, hello. I'm surprised there are so many skeletons.";
            text2 = "[fnt_pixel]Good thing they don't scare me. And they don't seem to be interested in me.";
            text3 = "[fnt_pixel]But look at all these mushrooms growing here. It's a good thing I have my favorite musical instrument, the [c_red]flute[/c].";
            text4 = "[fnt_pixel]I've noticed there's a lot of things you can do with a flute like that. I just got another one and I'm building a new test lane.";
            text5 = "[fnt_pixel]Want to try it out?";
            #endregion          
            break;
		case 1: //rus
			text1 = "[fnt_pixel_ru]Привет, привет. Странно что тут так много скелетов.";
			text2 = "[fnt_pixel_ru]Хорошо, что они меня не пугают. Да и я им, оказывается, особо не интересен.";
			text3 = "[fnt_pixel_ru]Но ты только посмотри какие забавные грибы тут растут! Хорошо что у меня под рукой оказался мой любимый музыкальный инструмент - [c_red]флейта[/c].";
			text4 = "[fnt_pixel_ru]Я заметил что тут много чего можно сделать с такой флейтой. У меня как раз есть еще одна и я построил новую полосу испытаний.";
			text5 = "[fnt_pixel_ru]Хочешь опробовать?";
			break;
		case 2: //es
			text1 = "[fnt_pixel]Hola, hola. Me sorprende que haya tantos esqueletos.";
			text2 = "[fnt_pixel]Menos mal que no me asustan. Y no parecen estar interesados en mí.";
			text3 = "[fnt_pixel]Pero mira todos estos hongos que crecen aquí. Menos mal que tengo mi instrumento musical favorito, [c_red]la flauta[/c].";
			text4 = "[fnt_pixel]He notado que hay muchas cosas que se pueden hacer con una flauta así. Acabo de conseguir otra y estoy construyendo una nueva pista de prueba.";
			text5 = "[fnt_pixel]¿Quieres probarlo?";
			break;
		case 3: //cn
			text1 = "[fnt_pixel]你好，你好。我很惊讶这里有这么多的骷髅。";
			text2 = "[fnt_pixel]好在他们不吓我。他们似乎对我也不感兴趣。";
			text3 = "[fnt_pixel]但是看看这些在这里生长的蘑菇。好在我有我最喜欢的乐器，[c_red]长笛[/c]。";
			text4 = "[fnt_pixel]我注意到用这样的长笛可以做很多事情。我刚弄来一支新的，我正在建造一个新的测试通道。";
			text5 = "[fnt_pixel]你想试试吗？";
			break;
		case 4: //pt
			text1 = "[fnt_pixel]Olá, olá. Estou surpreso com a quantidade de esqueletos.";
			text2 = "[fnt_pixel]Bom que eles não me assustam. E eles não parecem estar interessados em mim.";
			text3 = "[fnt_pixel]Mas olhe todos esses cogumelos crescendo aqui. É uma coisa boa eu ter meu instrumento musical favorito, a [c_red]flauta[/c].";
			text4 = "[fnt_pixel]Notei que há muitas coisas que você pode fazer com uma flauta assim. Acabei de receber outra e estou construindo uma nova pista de teste.";
			text5 = "[fnt_pixel]Quer experimentar?";
			break;
		case 5: //br
			text1 = "[fnt_pixel]Olá, olá. Estou surpreso que haja tantos esqueletos.";
			text2 = "[fnt_pixel]Ainda bem que eles não me assustam. E eles não parecem estar interessados em mim.";
			text3 = "[fnt_pixel]Mas olha todos esses cogumelos crescendo aqui. É uma coisa boa que eu tenha meu instrumento musical favorito, a [c_red]flauta[/c].";
			text4 = "[fnt_pixel]Eu notei que tem muitas coisas que você pode fazer com uma flauta assim. Acabei de conseguir outra e estou construindo uma nova pista de teste.";
			text5 = "[fnt_pixel]Quer experimentar?";
			break;
		case 6: //pl
			text1 = "[fnt_pixel]Cześć, cześć. Zdziwiony jestem, że jest tutaj tak wiele szkieletów.";
			text2 = "[fnt_pixel]Dobrze, że mnie nie przerażają. I wydaje się, że nie są mną zainteresowane.";
			text3 = "[fnt_pixel]Ale popatrz na te wszystkie grzyby, które tu rosną. Dobrze, że mam przy sobie moje ulubione muzyczne narzędzie, [c_red]flet[/c].";
			text4 = "[fnt_pixel]Zauważyłem, że jest wiele rzeczy, które można zrobić z takim fletem. Właśnie dostałem kolejny i buduję nowy tor testowy.";
			text5 = "[fnt_pixel]Chcesz to sprawdzić?";
			break;
		case 7: //jp
			text1 = "[fnt_pixel]こんにちは、こんにちは。そんなにたくさんのスケルトンがいるとは驚きです。";
			text2 = "[fnt_pixel]それらが私を怖がらせないのが良いことです。そして、彼らは私に興味がないようです。";
			text3 = "[fnt_pixel]しかし、ここに生えているキノコをすべて見てください。私のお気に入りの楽器、[c_red]フルート[/c]があるのは良いことです。";
			text4 = "[fnt_pixel]そのようなフルートでできることがたくさんあることに気づきました。私はもう一つ手に入れ、新しい試験レーンを建設しています。";
			text5 = "[fnt_pixel]試しにやってみたいか？";
			break;
		case 8: //de
			text1 = "[fnt_pixel]Hallo, hallo. Ich bin überrascht, dass es hier so viele Skelette gibt.";
			text2 = "[fnt_pixel]Gut, dass sie mich nicht erschrecken. Und sie scheinen auch kein Interesse an mir zu haben.";
			text3 = "[fnt_pixel]Aber schau dir all diese Pilze an, die hier wachsen. Gut, dass ich mein Lieblingsmusikinstrument, die [c_red]Flöte[/c], dabei habe.";
			text4 = "[fnt_pixel]Mir ist aufgefallen, dass man mit so einer Flöte eine Menge Dinge machen kann. Ich habe gerade eine weitere bekommen und baue eine neue Teststrecke.";
			text5 = "[fnt_pixel]Willst du es ausprobieren?";
			break;
		case 9: //fr
			text1 = "[fnt_pixel]Bonjour, bonjour. Je suis surpris qu'il y ait autant de squelettes.";
			text2 = "[fnt_pixel]Heureusement qu'ils ne me font pas peur. Et ils n'ont pas l'air intéressés par moi.";
			text3 = "[fnt_pixel]Mais regarde tous ces champignons qui poussent ici. Heureusement que j'ai mon instrument de musique préféré, la [c_red]flûte[/c].";
			text4 = "[fnt_pixel]J'ai remarqué qu'il y a beaucoup de choses que l'on peut faire avec une flûte comme celle-ci. Je viens d'en avoir une autre et je construis une nouvelle voie d'essai.";
			text5 = "[fnt_pixel]Tu veux essayer?";
			break;
		case 10: //it
			text1 = "[fnt_pixel]Ciao, ciao. Sono sorpreso che ci siano così tanti scheletri.";
			text2 = "[fnt_pixel]Per fortuna non mi fanno paura. E non sembrano interessati a me.";
			text3 = "[fnt_pixel]Ma guarda tutti questi funghi che crescono qui. Meno male che ho il mio strumento musicale preferito, il [c_red]flauto[/c].";
			text4 = "[fnt_pixel]Ho notato che c'è un sacco di cose che si possono fare con un flauto del genere. Ne ho appena ricevuto un altro e sto costruendo una nuova pista di prova.";
			text5 = "[fnt_pixel]Vuoi provarlo?";
			break;
    }
}

function scr_txt_shadow_inventor2(){
