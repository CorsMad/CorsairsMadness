function scr_txt_desert_pew3(){
	switch(global.language)
    {
        case 0: //English  
            #region  ENG text
            text1 = "[fnt_pixel]Oh, my God, what I've done. I followed the voice in my head and ended up here. ";
            text2 = "[fnt_pixel][wave]Everything else was a blur[/wave], but I remember what I said and how I felt.";
            text3 = "[fnt_pixel]I didn't mean no harm, you know me, Cap. ";
            text4 = "[fnt_pixel]I can't even imagine what I did up there.";
            text5 = "[fnt_pixel]I understand if you leave me here, but I still wanted to help you.";
            text6 = "[fnt_pixel]Please help me.";
            #endregion
            break;
        case 1: //russian
            text1 = "[fnt_pixel_ru]О боже, что я наделал. Я упорно шел за голосом в моей голове и очутился тут. ";
            text2 = "[fnt_pixel_ru][wave]Дальше все было как в тумане[/wave], однако я помню что говорил и что чувствовал.";
            text3 = "[fnt_pixel_ru]Я не хотел никому навредить, ты же знаешь меня, кэп.";
            text4 = "[fnt_pixel_ru]Даже не представляю что я натворил своими действиями там, наверху.";
            text5 = "[fnt_pixel_ru]Я пойму, если ты оставишь меня здесь, но все же я хотел еще помочь тебе.";
            text6 = "[fnt_pixel_ru]Пожалуйста помоги мне.";
            break;
        case 2: //spanish
            text1 = "[fnt_pixel]Oh, Dios mío, lo que he hecho. Seguí la voz en mi cabeza y terminé aquí.";
            text2 = "[fnt_pixel][wave]Todo lo demás era borroso[/wave], pero recuerdo lo que dije y cómo me sentí.";
            text3 = "[fnt_pixel]No quise hacer daño, me conoces, Cap.";
            text4 = "[fnt_pixel]Ni siquiera puedo imaginar lo que hice allí arriba.";
            text5 = "[fnt_pixel]Entiendo si me dejas aquí, pero todavía quería ayudarte.";
            text6 = "[fnt_pixel]Por favor, ayúdame.";
            break;
        case 3: //chinese
            text1 = "[fnt_pixel]哦，我的上帝，我做了什么。我听从了我头脑中的声音，最后来到了这里。";
            text2 = "[fnt_pixel][wave]其他的一切都模糊不清[/wave]，但我记得我说过什么，我是怎么感觉的。";
            text3 = "[fnt_pixel]我并不是有意要伤害，你知道我，Cap。";
            text4 = "[fnt_pixel]我甚至无法想象我在上面做了什么。";
            text5 = "[fnt_pixel]如果你把我留在这里，我能理解，但我还是想帮你。";
            text6 = "[fnt_pixel]请帮帮我。";
            break;
        case 4: //portug
            text1 = "[fnt_pixel]Meu Deus, o que eu fiz. Segui a voz na minha cabeça e acabei aqui.";
            text2 = "[fnt_pixel][wave]Tudo o resto estava embaçado[/wave], mas me lembro do que disse e como me senti.";
            text3 = "[fnt_pixel]Não quis fazer nenhum mal, você me conhece, Cap.";
            text4 = "[fnt_pixel]Não consigo nem imaginar o que fiz lá em cima.";
            text5 = "[fnt_pixel]Entendo se você me deixar aqui, mas ainda queria te ajudar.";
            text6 = "[fnt_pixel]Por favor me ajude.";
            break;
        case 5: //brazil
            text1 = "[fnt_pixel]Oh, meu Deus, o que eu fiz. Segui a voz na minha cabeça e acabei aqui.";
            text2 = "[fnt_pixel][wave]Tudo o mais estava desfocado[/wave], mas eu lembro do que eu disse e como me senti.";
            text3 = "[fnt_pixel]Eu não quis causar nenhum mal, você me conhece, Cap.";
            text4 = "[fnt_pixel]Eu nem consigo imaginar o que eu fiz lá em cima.";
            text5 = "[fnt_pixel]Eu entendo se você me deixar aqui, mas eu ainda queria te ajudar.";
            text6 = "[fnt_pixel]Por favor me ajude.";
            break;
        case 6: //polish
            text1 = "[fnt_pixel]O mój Boże, co ja zrobiłem. Poszedłem za głosem w mojej głowie i trafiłem tutaj. ";
            text2 = "[fnt_pixel][wave]Wszystko inne było jak we mgle[/wave], ale pamiętam, co powiedziałem i jak się czułem.";
            text3 = "[fnt_pixel]Nie chciałem wyrządzić krzywdy, wiesz, że tak jest, Kapitanie. ";
            text4 = "[fnt_pixel]Nie mogę nawet sobie wyobrazić, co zrobiłem tam na górze.";
            text5 = "[fnt_pixel]Rozumiem, jeśli zostawisz mnie tutaj, ale chciałem ci jeszcze pomóc.";
            text6 = "[fnt_pixel]Proszę, pomóż mi.";
            break;
        case 7: //japanese
            text1 = "[fnt_pixel]おお、神よ、私は何をしたのか。 私は頭の中の声に従い、ここに辿り着いた。";
            text2 = "[fnt_pixel][wave]他の全てがぼやけていて[/wave]、でも私が何を言ったか、どう感じたかは覚えています。";
            text3 = "[fnt_pixel]私が悪気があったわけではありません、あなたは私を知っています、キャプテン。";
            text4 = "[fnt_pixel]私がそこで何をしたのかさえ想像できません。";
            text5 = "[fnt_pixel]あなたが私をここに置いて行くことを理解していますが、私はあなたを助けたかっただけです。";
            text6 = "[fnt_pixel]私を助けてください。";
            break;
        case 8: //deutch
            text1 = "[fnt_pixel]Oh mein Gott, was habe ich getan. Ich folgte der Stimme in meinem Kopf und landete hier. ";
            text2 = "[fnt_pixel][wave]Alles andere war verschwommen[/wave], aber ich erinnere mich an das, was ich sagte und wie ich mich fühlte. ";
            text3 = "[fnt_pixel]Ich wollte keinen Schaden anrichten, du kennst mich, Cap. ";
            text4 = "[fnt_pixel]Ich kann mir gar nicht vorstellen, was ich dort oben getan habe. ";
            text5 = "[fnt_pixel]Ich verstehe, wenn du mich hier zurücklässt, aber ich wollte dir immer noch helfen. ";
            text6 = "[fnt_pixel]Bitte hilf mir.";
            break;
        case 9: //french
            text1 = "[fnt_pixel]Oh, mon Dieu, qu'est-ce que j'ai fait. J'ai suivi la voix dans ma tête et j'ai fini ici. ";
            text2 = "[fnt_pixel][wave]Tout le reste était flou[/wave], mais je me souviens de ce que j'ai dit et de ce que je ressentais.";
            text3 = "[fnt_pixel]Je ne voulais pas faire de mal, tu me connais, Cap. ";
            text4 = "[fnt_pixel]Je ne peux même pas imaginer ce que j'ai fait là-haut.";
            text5 = "[fnt_pixel]Je comprends si tu me laisses ici, mais je voulais quand même t'aider.";
            text6 = "[fnt_pixel]S'il te plaît, aide-moi.";
            break;
        case 10: //Italian
            text1 = "[fnt_pixel]Oh mio Dio, cosa ho fatto. Ho seguito la voce nella mia testa e sono finito qui.";
            text2 = "[fnt_pixel][wave]Tutto il resto era un'immagine sfuocata[/wave], ma ricordo quello che ho detto e come mi sentivo.";
            text3 = "[fnt_pixel]Non volevo fare del male, mi conosci, Cap.";
            text4 = "[fnt_pixel]Non riesco nemmeno a immaginare cosa ho fatto lì sopra.";
            text5 = "[fnt_pixel]Capisco se mi lasci qui, ma volevo ancora aiutarti.";
            text6 = "[fnt_pixel]Per favore aiutami.";
            break;
    }    
}		
	
function scr_txt_desert_pew4(){
