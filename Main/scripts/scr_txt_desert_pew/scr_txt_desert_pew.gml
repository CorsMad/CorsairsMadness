function scr_txt_desert_pew1(){
	switch(global.language)
    {
        case 0: //English  
            #region  ENG text
            text1 = "[fnt_pixel]It's just [wave]unimaginable[/wave]. Look at what I can do now, Captain!";
            text2 = "[fnt_pixel]I don't need a damn cane, I can walk on air, and I feel [wave]marvelous[/wave]!";
            text3 = "[fnt_pixel]But why are you here? Do you want to end all this?";
            text4 = "[fnt_pixel]Rest assured I won't let you do that. [shake]You will not take my power away from me![/shake]";
            #endregion
            break;
        case 1: //russian
            text1 = "[fnt_pixel_ru]Это просто [wave]невообразимо[/wave]. Только посмотри на что теперь я способен, капитан! ";
            text2 = "[fnt_pixel_ru]Мне не нужна проклятая трость, я могу передвигаться по воздуху и чувствую себя [wave]великолепно[/wave]!";
            text3 = "[fnt_pixel_ru]Но зачем ты здесь? Ты хочешь все это прекратить?";
            text4 = "[fnt_pixel_ru]Будь уверен, что я не позволю тебе этого сделать. [shake]Ты не отнимешь у меня мою силу![/shake]";
            break;
        case 2: //spanish
            text1 = "[fnt_pixel]Es simplemente [wave]inimaginable[/wave]. ¡Mira lo que puedo hacer ahora, Capitán!";
            text2 = "[fnt_pixel]No necesito una maldita caña, puedo caminar en el aire, ¡y me siento [wave]maravilloso[/wave]!";
            text3 = "[fnt_pixel]Pero, ¿por qué estás aquí? ¿Quieres poner fin a todo esto?";
            text4 = "[fnt_pixel]Descanse seguro de que no te dejaré hacer eso. [shake]¡No me quitarás mi poder![/shake]";
            break;
        case 3: //chinese
            text1 = "[fnt_pixel][wave]这简直无法想象[/wave]。看看我现在能做什么，船长！";
            text2 = "[fnt_pixel]我不需要那该死的手杖，我可以在空气中行走，[wave]我感觉棒极了[/wave]！";
            text3 = "[fnt_pixel]但是你为什么在这里？你想结束这一切吗？";
            text4 = "[fnt_pixel]放心，我不会让你这么做的。[shake]你不能把我的力量抢走！[/shake]"; 
            break;
        case 4: //portug
            text1 = "[fnt_pixel]É simplesmente [wave]inimaginável[/wave]. Olhe o que eu posso fazer agora, Capitão!";
            text2 = "[fnt_pixel]Eu não preciso de uma maldita bengala, eu posso andar no ar, e eu me sinto [wave]maravilhoso[/wave]!";
            text3 = "[fnt_pixel]Mas por que você está aqui? Você quer acabar com tudo isso?";
            text4 = "[fnt_pixel]Fique tranquilo que eu não vou permitir que você faça isso. [shake]Você não vai tirar meu poder de mim![/shake]"; 
            break;
        case 5: //brazil
            text1 = "[fnt_pixel]É simplesmente [wave]inimaginável[/wave]. Olhe o que eu posso fazer agora, Capitão!";
            text2 = "[fnt_pixel]Eu não preciso de uma maldita bengala, posso andar no ar, e me sinto [wave]maravilhoso[/wave]!";
            text3 = "[fnt_pixel]Mas por que você está aqui? Você quer acabar com tudo isso?";
            text4 = "[fnt_pixel]Tenha a certeza de que não vou deixar você fazer isso. [shake]Você não vai tirar meu poder de mim![/shake]"; 
            break;
        case 6: //polish
            text1 = "[fnt_pixel]To jest po prostu [wave]niewyobrażalne[/wave]. Spójrz, co teraz potrafię, Kapitanie!";
            text2 = "[fnt_pixel]Nie potrzebuję cholernego kija, mogę chodzić po powietrzu, a ja czuję się [wave]wspaniale[/wave]!";
            text3 = "[fnt_pixel]Ale czemu jesteś tutaj? Chcesz wszystko zakończyć?";
            text4 = "[fnt_pixel]Bądź pewien, że nie pozwolę ci na to. [shake]Nie odbierzesz mi mojej mocy![/shake]"; 
            break;
        case 7: //japanese
            text1 = "[fnt_pixel][wave]それはただ想像を絶します[/wave]。 今私が何ができるか見てください、キャプテン！";
            text2 = "[fnt_pixel]私はくそったれた杖など必要ない、空を歩くことができ、[wave]私はすばらしい気分です[/wave]！";
            text3 = "[fnt_pixel]でもなぜあなたはここにいるの？ 全部終わらせたいのですか？";
            text4 = "[fnt_pixel]安心してください、あなたがそれをするのは許しません。[shake]魔力を奪うには至りません！[/shake]"; 
            break;
        case 8: //deutch
            text1 = "[fnt_pixel]Das ist einfach [wave]unfassbar[/wave]. Schau mal, was ich jetzt kann, Captain! ";
            text2 = "[fnt_pixel]Ich brauche keinen verdammten Stock, ich kann auf der Luft laufen, und ich fühle mich [wave]fantastisch[/wave]! ";
            text3 = "[fnt_pixel]Aber warum bist du hier? Willst du all das beenden? ";
            text4 = "[fnt_pixel]Sei versichert, dass ich das nicht zulassen werde. [shake]Du wirst mir meine Macht nicht nehmen![/shake]"; 
            break;
        case 9: //french
            text1 = "[fnt_pixel]C'est simplement [wave]impensable[/wave]. Regarde ce que je peux faire maintenant, Capitaine!";
            text2 = "[fnt_pixel]Je n'ai pas besoin d'une foutue canne, je peux marcher dans l'air, et je me sens [wave]merveilleux[/wave]!";
            text3 = "[fnt_pixel]Mais pourquoi es-tu ici? Veux-tu mettre fin à tout ça?";
            text4 = "[fnt_pixel]Sois assuré que je ne te laisserai pas faire ça. [shake]Tu ne me prendras pas mon pouvoir![/shake]"; 
            break;
        case 10: //Italian
            text1 = "[fnt_pixel]È semplicemente [wave]inconcepibile[/wave]. Guarda cosa posso fare ora, Capitano!";
            text2 = "[fnt_pixel]Non mi serve un maledetto bastone, posso camminare sull'aria, e mi sento [wave]meraviglioso[/wave]!";
            text3 = "[fnt_pixel]Ma perché sei qui? Vuoi porre fine a tutto questo?";
            text4 = "[fnt_pixel]Sii certo che non ti permetterò di farlo. [shake]Non mi toglierai il mio potere![/shake]";            
            break;
    }    
}
	
