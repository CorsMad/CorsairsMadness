function scr_txt_jungle_chief1(){
    switch(global.language){
        case 0:
            #region English
            text1 = "[fnt_pixel]Welcome to our [c_green]jungle island[/c]. It's not often you see people from the big earth here. And today there are two!";
            text2 = "[fnt_pixel]We are here for a reason. For generations we have been guarding the local temple, where our ancestors once imprisoned terrible [wobble]dark forces[/wobble].";
            text3 = "[fnt_pixel]I understand your [c_red]friend[/c] was here before you? He didn't seem very friendly to us, because he immediately rushed into the jungle.";
            text4 = "[fnt_pixel]After a while, all the animals in the forest literally went crazy and started attacking my men!";
            text5 = "[fnt_pixel]He must have somehow infiltrated the temple and something bad happened.";
            text6 = "[fnt_pixel]While you're here, why don't you find out what happened to that guy?";
            text7 = "[fnt_pixel]To get into the temple you need to find [c_red]two keys[/c] that are hidden on this island.";
            text8 = "[fnt_pixel]I hope you don't have any trouble finding them.";
            text9 = "[fnt_pixel]Before you go - visit the local [c_red]inventor[/c], maybe he can help you with some device.";
            text10 ="[fnt_pixel]Good luck!";
            #endregion
            break;
		case 1:
			#region rus
			text1 =  "[fnt_pixel_ru]О приветствую на нашем [c_green]острове джунглей[/c]. Нечасто здесь встретишь людей с большой земли. А сегодня целых двое!";
			text2 =  "[fnt_pixel_ru]Мы тут неспроста. Уже много поколений охраняем местный храм, в котором когда то наши предки заточили страшные [wobble]темные силы[/wobble].";
			text3 =  "[fnt_pixel_ru]Насколько я понял до тебя тут был твой [c_red]друг[/c]? Он показался нам не особо приветливым, потому что сразу ринулся в джунгли.";
			text4 =  "[fnt_pixel_ru]Через некоторое время все животные в лесу буквально сошли с ума и начали атаковать моих людей!";
			text5 =  "[fnt_pixel_ru]Видимо он каким то образом проник в храм и произошло что-то нехорошее.";
			text6 =  "[fnt_pixel_ru]Раз уж ты тут, может быть ты разузнаешь что произошло с тем парнем?";
			text7 =  "[fnt_pixel_ru]Чтобы попасть в храм тебе необходимо найти [c_red]2 ключа[/c], которые спрятаны ну этом острове.";
			text8 =  "[fnt_pixel_ru]Надеюсь, тебе не составит труда найти их.";
			text9 =  "[fnt_pixel_ru]Пока ты не ушел - навести местного [c_red]изобретателя[/c], возможно он сможет помочь тебе каким-нибудь устройством.";
			text10 = "[fnt_pixel_ru]Удачи!";
			#endregion
			break;
		case 2:
			#region esp

			text1 =  "[fnt_pixel]Bienvenido a nuestra [c_green]isla de la jungla[/c]. No es frecuente ver a gente de la gran tierra aquí. ¡Y hoy hay dos!";
			text2 =  "[fnt_pixel]Estamos aquí por una razón. Durante generaciones hemos estado custodiando el templo local, donde nuestros antecesores una vez encerraron [wobble]fuerzas oscuras terribles[/wobble].";
			text3 =  "[fnt_pixel]¿Entiendo que tu [c_red]amigo[/c] estuvo aquí antes que tú? No nos pareció muy amistoso, porque inmediatamente se precipitó hacia la selva.";
			text4 =  "[fnt_pixel]Al cabo de un rato, todos los animales del bosque literalmente enloquecieron y comenzaron a atacar a mis hombres!";
			text5 =  "[fnt_pixel]Debe haberse infiltrado de alguna manera en el templo y algo malo sucedió.";
			text6 =  "[fnt_pixel]Mientras estás aquí, ¿por qué no averiguas qué le pasó a ese tipo?";
			text7 =  "[fnt_pixel]Para entrar al templo necesitas encontrar [c_red]dos llaves[/c] que están escondidas en esta isla.";
			text8 =  "[fnt_pixel]Espero que no tengas problemas para encontrarlas.";
			text9 =  "[fnt_pixel]Antes de que te vayas, visita al [c_red]inventor[/c] local, quizás pueda ayudarte con algún dispositivo.";
			text10 = "[fnt_pixel]¡Buena suerte!";
			#endregion
			break;
		case 3:
			#region chinese
			text1 =  "[fnt_pixel]欢迎来到我们的[c_green]丛林岛[/c]。鲜有人从大地之上来到这里。而今天来了两位！";
			text2 =  "[fnt_pixel]我们在这儿是有原因的。几代人以来，我们一直在守护着当地的神庙，我们的祖先曾在那里关押了恐怖的[wobble]黑暗力量[/wobble]。";
			text3 =  "[fnt_pixel]我知道[c_red]你的朋[/c]友在你之前来过这里吗？他对我们并不友好，因为他立刻就冲进了丛林。";
			text4 =  "[fnt_pixel]过了一会儿，森林里的所有动物几乎都疯了，开始攻击我的人！";
			text5 =  "[fnt_pixel]他一定是以某种方式潜入了神庙，然后出了什么事。";
			text6 =  "[fnt_pixel]既然你在这儿，为什么不去找出那个家伙发生了什么事呢？";
			text7 =  "[fnt_pixel]要进入神庙，你需要找到隐藏在这个岛上的[c_red]两把钥匙[/c]。";
			text8 =  "[fnt_pixel]我希望你找这些钥匙不会有任何困难。";
			text9 =  "[fnt_pixel]在你离开之前 - 去拜访一下[c_red]当地的发明家[/c]，也许他可以用一些设备帮助你。";
			text10 = "[fnt_pixel]祝你好运！";
			#endregion
			break;
		case 4:
			#region port
			text1 =  "[fnt_pixel]Bem-vindo à nossa [c_green]ilha da selva[/c]. Não é comum ver pessoas da grande terra aqui. E hoje existem dois! ";
			text2 =  "[fnt_pixel]Estamos aqui por um motivo. Por gerações, temos protegido o templo local, onde nossos ancestrais certa vez aprisionaram forças [wobble]sombrias terríveis[/wobble].";
			text3 =  "[fnt_pixel]Eu entendo que seu [c_red]amigo[/c] esteve aqui antes de você? Ele não nos parecia muito amigável, porque imediatamente correu para a selva. ";
			text4 =  "[fnt_pixel]Depois de um tempo, todos os animais da floresta literalmente enlouqueceram e começaram a atacar meus homens! ";
			text5 =  "[fnt_pixel]Ele deve ter de alguma forma se infiltrado no templo e algo ruim aconteceu. ";
			text6 =  "[fnt_pixel]Enquanto você está aqui, por que não descobre o que aconteceu com aquele cara? ";
			text7 =  "[fnt_pixel]Para entrar no templo, você precisa encontrar [c_red]duas chaves[/c] que estão escondidas nesta ilha. ";
			text8 =  "[fnt_pixel]Espero que você não tenha problemas para encontrá-las. ";
			text9 =  "[fnt_pixel]Antes de ir - visite o [c_red]inventor[/c] local, talvez ele possa te ajudar com algum dispositivo. ";
			text10 = "[fnt_pixel]Boa sorte!";
			#endregion
			break;
		case 5:
			#region braz
			text1 =  "[fnt_pixel]Bem-vindo à nossa [c_green]ilha da selva[/c]. Não é frequente ver pessoas do grande mundo aqui. E hoje temos duas!";
			text2 =  "[fnt_pixel]Estamos aqui por um motivo. Por gerações, temos guardado o templo local, onde nossos ancestrais uma vez aprisionaram terríveis [wobble]forças obscuras[wobble/].";
			text3 =  "[fnt_pixel]Entendo que o seu [c_red]amigo[/c] esteve aqui antes de você? Ele não pareceu muito amigável para nós, porque correu imediatamente para a selva.";
			text4 =  "[fnt_pixel]Depois de um tempo, todos os animais da floresta literalmente enlouqueceram e começaram a atacar os meus homens!";
			text5 =  "[fnt_pixel]Ele deve ter de alguma forma infiltrado o templo e algo ruim aconteceu.";
			text6 =  "[fnt_pixel]Enquanto você está aqui, por que não descobre o que aconteceu com aquele cara?";
			text7 =  "[fnt_pixel]Para entrar no templo você precisa encontrar [c_red]duas chaves[/c] que estão escondidas nesta ilha.";
			text8 =  "[fnt_pixel]Espero que você não tenha nenhum problema para encontrá-las.";
			text9 =  "[fnt_pixel]Antes de você ir - visite o [c_red]inventor[/c] local, talvez ele possa te ajudar com algum dispositivo.";
			text10 = "[fnt_pixel]Boa sorte!";
			#endregion
			break;
		case 6:
			#region pl
			text1 =  "[fnt_pixel]Witamy na naszej [c_green]wyspie dżungli[/c]. Rzadko zdarza się, żebyśmy widzieli tu ludzi z dużego lądu. A dzisiaj jest ich dwóch!";
			text2 =  "[fnt_pixel]Jesteśmy tu z jakiegoś powodu. Od pokoleń strzeżemy miejscowej świątyni, gdzie nasi przodkowie kiedyś uwięzili straszliwe [wobble]ciemne siły[/wobble].";
			text3 =  "[fnt_pixel]Rozumiem, że twój [c_red]przyjaciel[/c] był tu przed tobą? Nie wyglądał na zbyt przyjaznego dla nas, bo natychmiast rzucił się do dżungli.";
			text4 =  "[fnt_pixel]Po chwili wszystkie zwierzęta w lesie dosłownie oszalały i zaczęły atakować moich ludzi!";
			text5 =  "[fnt_pixel]Musiał jakoś włamać się do świątyni i coś złego się wydarzyło.";
			text6 =  "[fnt_pixel]Skoro już tu jesteś, dlaczego nie dowiesz się, co się stało z tym gościem?";
			text7 =  "[fnt_pixel]Aby dostać się do świątyni, musisz znaleźć [c_red]dwie klucze[/c], które są ukryte na tej wyspie.";
			text8 =  "[fnt_pixel]Mam nadzieję, że nie będziesz miał problemów ze znalezieniem ich.";
			text9 =  "[fnt_pixel]Zanim pójdziesz - odwiedź miejscowego [c_red]wynalazcę[/c], może on pomoże ci jakimś urządzeniem.";
			text10 = "[fnt_pixel]Powodzenia!";
			#endregion
			break;
		case 7:
			#region jp
			text1 =  "[fnt_pixel]私たちの[c_green]ジャングル島[/c]へようこそ。大地からの人々がここに来るのは珍しい。そして今日は二人も！";
			text2 =  "[fnt_pixel]我々は理由があってここにいます。世代を重ねて、我々は地元の寺院を守り続けてきました。ここには先祖が一度、[wobble]恐ろしい闇の力を[/wobble]閉じ込めたのです。";
			text3 =  "[fnt_pixel]あなたの[c_red]友人[/c]はあなたより先にここに来たのですね？彼は我々に対して非常に友好的ではなかったようで、すぐにジャングルに突進していきました。";
			text4 =  "[fnt_pixel]しばらくすると、森の中の動物たちが文字通り狂って、私の部下たちを襲い始めたんです！";
			text5 =  "[fnt_pixel]彼は何かしらの方法で寺院に侵入し、何か悪いことが起こったに違いありません。";
			text6 =  "[fnt_pixel]ここにいる間、その男が何が起こったのかを調べてみてはいかがでしょうか？";
			text7 =  "[fnt_pixel]寺院に入るには、この島に隠された[c_red]二つの鍵[/c]を見つける必要があります。";
			text8 =  "[fnt_pixel]その探し物に困ることはないと思います。";
			text9 =  "[fnt_pixel]出かける前に、地元の[c_red]発明家[/c]を訪れてみてください。彼が何かデバイスで助けてくれるかもしれません。";
			text10 = "[fnt_pixel]幸運を祈ります！";
			#endregion
			break;
		case 8:
			#region de
			text1 =  "[fnt_pixel]Willkommen auf unserer [c_green]Dschungelinsel[/c]. Es ist nicht oft, dass wir hier Menschen von der großen Erde sehen. Und heute sind es sogar zwei!";
			text2 =  "[fnt_pixel]Wir sind hier aus einem Grund. Seit Generationen bewachen wir den örtlichen Tempel, in dem unsere Vorfahren einst schreckliche [wobble]dunkle Kräfte[/wobble] eingesperrt haben.";
			text3 =  "[fnt_pixel]Ich verstehe, dein [c_red]Freund[/c] war vor dir hier? Er schien uns nicht sehr freundlich gesinnt, weil er sofort in den Dschungel stürmte.";
			text4 =  "[fnt_pixel]Nach einer Weile wurden alle Tiere im Wald buchstäblich verrückt und begannen, meine Männer anzugreifen!";
			text5 =  "[fnt_pixel]Er muss irgendwie in den Tempel eingedrungen sein und etwas Schlimmes ist passiert.";
			text6 =  "[fnt_pixel]Solange du hier bist, warum findest du nicht heraus, was mit diesem Kerl passiert ist?";
			text7 =  "[fnt_pixel]Um in den Tempel zu gelangen, müssen Sie [c_red]zwei Schlüssel[/c] finden, die auf dieser Insel versteckt sind.";
			text8 =  "[fnt_pixel]Ich hoffe, Sie haben keine Probleme, diese zu finden.";
			text9 =  "[fnt_pixel]Bevor du gehst - besuche den örtlichen [c_red]Erfinder[/c], vielleicht kann er dir mit irgendeinem Gerät helfen.";
			text10 = "[fnt_pixel]Viel Glück!";
			#endregion
			break;
		case 9:
			#region fr
			text1 =  "[fnt_pixel]Bienvenue sur notre [c_green]île de la jungle[/c]. On ne voit pas souvent des gens de la grande terre ici. Et aujourd'hui, il y en a deux !";
			text2 =  "[fnt_pixel]Nous sommes ici pour une raison. Depuis des générations, nous gardons le temple local, où nos ancêtres ont autrefois emprisonné de terribles [wobble]forces obscures[/wobble].";
			text3 =  "[fnt_pixel]Je comprends que votre [c_red]ami[/c] était là avant vous ? Il ne nous a pas semblé très sympathique, car il s'est immédiatement précipité dans la jungle.";
			text4 =  "[fnt_pixel]Au bout d'un moment, tous les animaux de la forêt sont littéralement devenus fous et ont commencé à attaquer mes hommes !";
			text5 =  "[fnt_pixel]Il a dû d'une manière ou d'une autre s'introduire dans le temple et quelque chose de mauvais s'est produit.";
			text6 =  "[fnt_pixel]Tant que vous êtes ici, pourquoi ne pas découvrir ce qui est arrivé à ce type ?";
			text7 =  "[fnt_pixel]Pour entrer dans le temple, vous devez trouver [c_red]deux clés[/c] qui sont cachées sur cette île.";
			text8 =  "[fnt_pixel]J'espère que vous n'aurez pas de problème pour les trouver.";
			text9 =  "[fnt_pixel]Avant de partir - visitez [c_red]l'inventeur[/c] local, peut-être qu'il peut vous aider avec un certain appareil.";
			text10 = "[fnt_pixel]Bonne chance !";
			#endregion
			break;
		case 10:
			#region it
			text1 =  "[fnt_pixel]Benvenuto nella nostra [c_green]isola della giungla[/c]. Non è frequente vedere persone dalla grande terra qui. E oggi ce ne sono due!";
			text2 =  "[fnt_pixel]Siamo qui per un motivo. Da generazioni stiamo custodendo il tempio locale, dove i nostri antenati hanno una volta imprigionato terribili [wobble]forze oscure[/wobble].";
			text3 =  "[fnt_pixel]Capisco che il tuo [c_red]amico[/c] era qui prima di te? Non ci è sembrato molto amichevole, perché si è subito precipitato nella giungla.";
			text4 =  "[fnt_pixel]Dopo un po', tutti gli animali nella foresta sono letteralmente impazziti e hanno iniziato ad attaccare i miei uomini!";
			text5 =  "[fnt_pixel]Deve aver in qualche modo infiltrato il tempio e qualcosa di brutto è successo.";
			text6 =  "[fnt_pixel]Mentre sei qui, perché non scopri cosa è successo a quel ragazzo?";
			text7 =  "[fnt_pixel]Per entrare nel tempio è necessario trovare [c_red]due chiavi[/c] che sono nascoste su quest'isola.";
			text8 =  "[fnt_pixel]Spero che non avrai problemi a trovare quelli.";
			text9 =  "[fnt_pixel]Prima di andare - visita [c_red]l'inventore[/c] locale, forse può aiutarti con qualche dispositivo.";
			text10 = "[fnt_pixel]Buona fortuna!";
			#endregion
			break;
	
    }
}

function scr_txt_jungle_chief2(){
    switch(global.language){
        case 0:
            #region English
            text1 = "[fnt_pixel]Ugh thank you so much for dispelling the sorcery. Now we can take a little breather.";
            text2 = "[fnt_pixel]I will send the guards to their posts in the jungle.";
            text3 = "[fnt_pixel]And I want to say thank you! I hope your man is all right.";
            text4 = "[fnt_pixel]Here's a parting [c_yellow]gift[/c] for you.";
            #endregion
            break;
		case 1:
			#region rus
			text1 = "[fnt_pixel_ru]Уф огромное спасибо за то что чары развеяны. Теперь можно немного перевести дух.";
			text2 = "[fnt_pixel_ru]Я отправлю стражников на свои посты в джунгли.";
			text3 = "[fnt_pixel_ru]А тебе хочу сказать спасибо! Надеюсь с твоим человеком все в порядке.";
			text4 = "[fnt_pixel_ru]Вот тебе [c_yellow]подарок[/c] на прощание.";
			#endregion
			break;
		case 2:
			#region esp
			text1 = "[fnt_pixel]Ugh, muchas gracias por disipar la brujería. Ahora podemos tomar un pequeño respiro.";
			text2 = "[fnt_pixel]Enviaré a los guardias a sus puestos en la selva.";
			text3 = "[fnt_pixel]¡Y quiero decirte gracias! Espero que tu hombre esté bien.";
			text4 = "[fnt_pixel]Aquí tienes un [c_yellow]regalo[/c] de despedida.";
			#endregion
			break;
		case 3:
			#region chinese
			text1 = "[fnt_pixel]啊，非常感谢你解除了那个魔法。现在我们可以稍微喘口气了。";
			text2 = "[fnt_pixel]我会把警卫派去丛林的哨位。";
			text3 = "[fnt_pixel]我想说谢谢你！我希望你的朋友没事。";
			text4 = "[fnt_pixel]这是给你的离别[c_yellow]礼物[/c]。";
			#endregion
			break;
		case 4:
			#region port
			text1 = "[fnt_pixel]Ugh, muito obrigado por dissipar a feitiçaria. Agora podemos respirar um pouco.";
			text2 = "[fnt_pixel]Vou enviar os guardas para seus postos na selva.";
			text3 = "[fnt_pixel]E quero dizer obrigado! Espero que seu homem esteja bem.";
			text4 = "[fnt_pixel]Aqui está um [c_yellow]presente[/c] de despedida para você.";
			#endregion
			break;
		case 5:
			#region braz
			text1 = "[fnt_pixel]Ugh, muito obrigado por dissipar a feitiçaria. Agora podemos respirar um pouco.";
			text2 = "[fnt_pixel]Vou enviar os guardas para os seus postos na selva.";
			text3 = "[fnt_pixel]E eu quero dizer obrigado! Espero que o seu homem esteja bem.";
			text4 = "[fnt_pixel]Aqui está um [c_yellow]presente[/c] de despedida para você.";
			#endregion
			break;
		case 6:
			#region pl
			text1 = "[fnt_pixel]Uch dziękuję Ci bardzo za rozwianie czarów. Teraz możemy trochę odpocząć.";
			text2 = "[fnt_pixel]Wyślę strażników na ich stanowiska w dżungli.";
			text3 = "[fnt_pixel]I chcę powiedzieć dziękuję! Mam nadzieję, że twój człowiek jest w porządku.";
			text4 = "[fnt_pixel]Oto [c_yellow]prezent[/c] pożegnalny dla ciebie.";
			#endregion
			break;
		case 7:
			#region jp
			text1 = "[fnt_pixel]うぅ、魔法を解いてくれて本当にありがとう。これで少し一息つくことができます。";
			text2 = "[fnt_pixel]私はガードたちをジャングルの彼らのポストに送ります。";
			text3 = "[fnt_pixel]そして、ありがとうと言いたい！ あなたの男性が大丈夫であることを願っています。";
			text4 = "[fnt_pixel]これはあなたへのお[c_yellow]別れの贈[/c]り物です。";
			#endregion
			break;
		case 8:
			#region de
			text1 = "[fnt_pixel]Ugh, vielen Dank, dass Sie die Zauberei vertrieben haben. Jetzt können wir ein bisschen durchatmen.";
			text2 = "[fnt_pixel]Ich werde die Wachen an ihre Posten im Dschungel schicken.";
			text3 = "[fnt_pixel]Und ich möchte Danke sagen! Ich hoffe, es geht Ihrem Mann gut.";
			text4 = "[fnt_pixel]Hier ist ein [c_yellow]Abschiedsgeschenk[/c] für Sie.";
			#endregion
			break;
		case 9:
			#region fr
			text1 = "[fnt_pixel]Ugh, merci beaucoup d'avoir dissipé la sorcellerie. Maintenant, nous pouvons prendre un peu de répit.";
			text2 = "[fnt_pixel]J'enverrai les gardes à leurs postes dans la jungle.";
			text3 = "[fnt_pixel]Et je veux dire merci ! J'espère que votre homme va bien.";
			text4 = "[fnt_pixel]Voici un [c_yellow]cadeau[/c] de départ pour vous.";
			#endregion
			break;
		case 10:
			#region it
			text1 = "[fnt_pixel]Oh, grazie mille per aver dissipato la stregoneria. Ora possiamo prenderci un piccolo respiro.";
			text2 = "[fnt_pixel]Manderò le guardie ai loro posti nella giungla.";
			text3 = "[fnt_pixel]E voglio dire grazie! Spero che il tuo uomo stia bene.";
			text4 = "[fnt_pixel]Ecco un [c_yellow]regalo[/c] di addio per te.";
			#endregion
			break;
    }
}