			text3 = "[fnt_pixel]我听说如果你点亮[c_red]所有的水晶[/c]，就会出现一个宝箱。";
			text4 = "[fnt_pixel]不幸的是，那里有高度的熔岩活动，所以要睁大眼睛看！";
			break;
		case 4: //pt
			text1 = "[fnt_pixel]Nossos [c_red]cristais[/c] acendem brevemente se você bater neles.";
			text2 = "[fnt_pixel]Mas em uma pequena ilha próxima há uma caverna em que esses cristais [c_red]nunca param de brilhar[/c]!";
			text3 = "[fnt_pixel]Ouvi dizer que se você acender todos os [c_red]cristais[/c], então aparecerá um baú de tesouro.";
			text4 = "[fnt_pixel]Infelizmente, há grande atividade de lava, então fique de olho!";
			break;
		case 5: //br
			text1 = "[fnt_pixel]Nossos [c_red]cristais[/c] acendem brevemente se você bater neles.";
			text2 = "[fnt_pixel]Mas em uma pequena ilha por perto existe uma caverna em que esses cristais [c_red]nunca param de brilhar[/c]!";
			text3 = "[fnt_pixel]Ouvi dizer que se você acender todos os [c_red]cristais[/c], então um baú de tesouros aparecerá.";
			text4 = "[fnt_pixel]Infelizmente, há alta atividade de lava, então mantenha os olhos abertos!";
			break;
		case 6: //pl
			text1 = "[fnt_pixel]Nasze [c_red]kryształy[/c] krótko świecą, jeśli je uderzysz.";
			text2 = "[fnt_pixel]Ale na małej wysepce obok jest jaskinia, w której te kryształy [c_red]nigdy nie przestają świecić[/c]!";
			text3 = "[fnt_pixel]Słyszałem, że jeśli zapalisz wszystkie [c_red]kryształy[/c], pojawi się skrzynia ze skarbem.";
			text4 = "[fnt_pixel]Niestety, jest tam duża aktywność lawy, więc miej oczy szeroko otwarte!";
			break;
		case 7: //jp
			text1 = "[fnt_pixel][c_red]々のクリスタル[/c]はあなたがそれらに当たると一瞬だけ明るくなります。";
			text2 = "[fnt_pixel]でも近くの小島には、これらのクリスタルが[c_red]決して光り続ける[/c]洞窟があります！";
			text3 = "[fnt_pixel][c_red]クリスタル[/c]すべてを点灯すると、宝箱が現れると聞きました。";
			text4 = "[fnt_pixel]残念ながら、溶岩活動が多いので、目を開けておいてください！";
			break;
		case 8: //de
			text1 = "[fnt_pixel]Unsere [c_red]Kristalle[/c] leuchten kurz auf, wenn du sie triffst.";
			text2 = "[fnt_pixel]Aber auf einer kleinen Insel in der Nähe gibt es eine Höhle, in der diese Kristalle [c_red]nie aufhören zu leuchten[/c]!";
			text3 = "[fnt_pixel]Ich habe gehört, dass, wenn du alle [c_red]Kristalle[/c] erleuchtest, dann erscheint eine Schatzkiste.";
			text4 = "[fnt_pixel]Leider gibt es dort hohe Lavaaktivität, also halte deine Augen offen!";
			break;
		case 9: //fr
			text1 = "[fnt_pixel]Nos [c_red]cristaux[/c] s'allument brièvement si vous les frappez.";
			text2 = "[fnt_pixel]Mais sur une petite île à proximité, il y a une grotte dans laquelle ces cristaux [c_red]ne cessent jamais de briller[/c]!";
			text3 = "[fnt_pixel]J'ai entendu dire que si vous allumez tous les [c_red]cristaux[/c], alors un coffre au trésor apparaîtra.";
			text4 = "[fnt_pixel]Malheureusement, l'activité de la lave est élevée, alors gardez les yeux ouverts!";
			break;
		case 10: //it
			text1 = "[fnt_pixel]I nostri [c_red]cristalli[/c] si illuminano brevemente se li colpisci.";
			text2 = "[fnt_pixel]Ma su una piccola isola vicina c'è una grotta in cui questi cristalli [c_red]non smettono mai di brillare[/c]!";
			text3 = "[fnt_pixel]Ho sentito dire che se accendi tutti i [c_red]cristalli[/c], apparirà un baule del tesoro.";
			text4 = "[fnt_pixel]Purtroppo, c'è alta attività lavica, quindi tieni gli occhi aperti!";
			break;
    }
}

