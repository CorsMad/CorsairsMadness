function scr_txt_snow_inventor1(){
    switch(global.language){
        case 0:
        #region English
            text1 = "[fnt_pixel]Hi! Aren't you cold? That's weird.";
            text2 = "[fnt_pixel]I had to make special heated spears for the locals.";
            text3 = "[fnt_pixel]By the way, even though it's all mountains, I found something to entertain myself. Look at these [c_red]bladed boots[/c]!";
