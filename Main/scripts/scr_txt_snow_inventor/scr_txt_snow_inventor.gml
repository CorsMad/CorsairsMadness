		case 7: //jp
			text1 = "[fnt_pixel]こんにちは！ 寒くないの？それは奇妙です。";
			text2 = "[fnt_pixel]私は地元の人々のために特別な加熱された槍を作らなければなりませんでした。";
			text3 = "[fnt_pixel]ところで、全てが山でも、自分を楽しませる何かを見つけました。 [c_red]これらのブレード付きブーツを見てください[/c]！";
			text4 = "[fnt_pixel][wave]それらは一部の表面で非常に簡単に滑ることができます[/wave]。 2組作りました。そしてもちろん、新しい障害物コースも作りました！";
			text5 = "[fnt_pixel]試してみたいですか？";
			break;
		case 8: //de
			text1 = "[fnt_pixel]Hallo! Ist dir nicht kalt? Das ist merkwürdig. ";
			text2 = "[fnt_pixel]Ich musste spezielle beheizte Speere für die Einheimischen herstellen. ";
			text3 = "[fnt_pixel]Übrigens, obwohl es lauter Berge sind, habe ich etwas gefunden, um mich zu unterhalten. Schau dir diese [c_red]Stiefel mit Klingen[/c] an!";
			text4 = "[fnt_pixel][wave]Sie gleiten unglaublich leicht über einige Oberflächen[/wave]. Ich habe 2 Paare gemacht und natürlich einen neuen Hindernisparcours!";
			text5 = "[fnt_pixel]Willst du es ausprobieren?";
			break;
		case 9: //fr
			text1 = "[fnt_pixel]Salut! Tu n'as pas froid? C'est bizarre.";
			text2 = "[fnt_pixel]J'ai dû fabriquer des lances chauffantes spéciales pour les habitants locaux.";
			text3 = "[fnt_pixel]Au fait, même si c'est tout des montagnes, j'ai trouvé quelque chose pour m'amuser. [c_red]Regarde ces bottes à lames[/c]!";
			text4 = "[fnt_pixel][wave]Elles sont incroyablement faciles à glisser sur certaines surfaces[/wave]. J'en ai fabriqué 2 paires et, bien sûr, un nouveau parcours d'obstacles!";
			text5 = "[fnt_pixel]Tu veux essayer?";
			break;
		case 10: //it
			text1 = "[fnt_pixel]Ciao! Non hai freddo? È strano.";
			text2 = "[fnt_pixel]Ho dovuto fare delle lance riscaldate speciali per i locali.";
			text3 = "[fnt_pixel]A proposito, anche se sono tutte montagne, ho trovato qualcosa per divertirmi. [c_red]Guarda questi stivali lama[/c]!";
			text4 = "[fnt_pixel][wave]È incredibilmente facile scivolare su alcune superfici[/wave]. Ho fatto 2 paia e, ovviamente, un nuovo percorso ad ostacoli!";
			text5 = "[fnt_pixel]Vuoi provarlo?";
			break;
    }
}

