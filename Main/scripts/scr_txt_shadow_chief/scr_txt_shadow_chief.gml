function scr_txt_shadow_chief1(){
    switch(global.language){
        case 0:
            #region English
            text1 = "[fnt_pixel]Hello! Welcome to our [c_purple]shadow island[/c]! As you can see, it's chaos outside!";
            text2 = "[fnt_pixel]The streets are overrun with [shake]skeletons[/shake]. And it all started after one [c_red]big guy[/c] sneaked past the village.";
            text3 = "[fnt_pixel]I bet it's someone you know, because our visitors don't usually move around on their own anymore, heh heh heh.";
            text4 = "[fnt_pixel]That's because our island is a big [c_red]Graveyard[/c] where people are brought to be buried.";
            text5 = "[fnt_pixel]But these skeletons will scare away all the people and they'll stop visiting us altogether. Why don't you go to our temple and deal with it?";
            #endregion
            break;
		case 1: //rus
			text1 = "[fnt_pixel_ru]Привет! Добро пожаловать на наш [c_purple]теневой остров[/c]! Как видишь на улице полнейший хаос! ";
			text2 = "[fnt_pixel_ru]Улицы заполонили толпы [shake]скелетов[/shake]. И все это началось после того как один [c_red]бугай[/c] пробирался мимо деревни.";
			text3 = "[fnt_pixel_ru]Держу пари это твой знакомый, потому что наши посетители обычно сами уже не передвигаются хе хе хе.";
			text4 = "[fnt_pixel_ru]А все потому что наш остров это большое [c_red]Кладблище[/c], куда привозят людей для захоронения.";
			text5 = "[fnt_pixel_ru]Но эти скелеты распугают весь народ и нас совсем перстанут посещать. Может отправишься в наш храм и разберешься с этим?";
			break;
		case 2: //es
			text1 = "[fnt_pixel]¡Hola! ¡Bienvenido a nuestra [c_purple]isla de las sombras[/c]! Como puedes ver, ¡es un caos afuera!";
			text2 = "[fnt_pixel]Las calles están llenas de [shake]esqueletos[/shake]. Y todo empezó después de que un [c_red]gran tipo[/c] se escabulló por el pueblo.";
			text3 = "[fnt_pixel]Apuesto a que es alguien que conoces, porque nuestros visitantes no suelen moverse por su cuenta más, je je je.";
			text4 = "[fnt_pixel]Eso es porque nuestra isla es un gran [c_red]cementerio[/c] donde se trae a la gente para ser enterrada.";
			text5 = "[fnt_pixel]Pero estos esqueletos asustarán a todas las personas y dejarán de visitarnos por completo. ¿Por qué no vas a nuestro templo y te encargas de ello?";
			break;
		case 3: //cn
			text1 = "[fnt_pixel]您好！欢迎来到我们[c_purple]的影子岛[/c]！如你所见，外面一片混乱！";
			text2 = "[fnt_pixel]街道上都是[shake]骷髅[/shake]。这都是一[c_red]个大家伙悄悄[/c]的过村子之后开始的。";
			text3 = "[fnt_pixel]我敢打赌这是你知道的人，因为我们的访客通常不会自己动的，呵呵呵。";
			text4 = "[fnt_pixel]这是因为我们的岛是一[c]个大墓地[/c]，人们被带来这里埋葬。";
			text5 = "[fnt_pixel]但是这些骷髅会把所有人都吓跑，他们会停止再来访问我们。你为什么不去我们的寺庙处理一下呢？";
			break;
		case 4: //pt
			text1 = "[fnt_pixel]Olá! Bem-vindo à nossa [c_purple]ilha sombria[/c]! Como pode ver, é o caos lá fora!";
			text2 = "[fnt_pixel]As ruas estão superlotadas de [shake]esqueletos[/shake]. E tudo começou depois que um [c_red]sujeito grande[/c] passou sorrateiramente pela aldeia.";
			text3 = "[fnt_pixel]Aposto que é alguém que você conhece, porque nossos visitantes geralmente não se movem mais por conta própria, heh heh heh.";
			text4 = "[fnt_pixel]Isso porque nossa ilha é um grande [c_red]cemitério[/c] onde as pessoas são trazidas para serem enterradas.";
			text5 = "[fnt_pixel]Mas esses esqueletos vão espantar todas as pessoas e elas vão parar de nos visitar completamente. Por que você não vai ao nosso templo e resolve isso?";
			break;
		case 5: //br
			text1 = "[fnt_pixel]Olá! Bem-vindo à nossa [c_purple]ilha sombria[/c]! Como você pode ver, é um caos lá fora!";
			text2 = "[fnt_pixel]As ruas estão infestadas de [shake]esqueletos[/shake]. E tudo começou depois que um [c_red]cara grande[/c] conseguiu passar pela aldeia.";
			text3 = "[fnt_pixel]Aposto que é alguém que você conhece, porque nossos visitantes geralmente não se movem mais por conta própria, heh heh heh.";
			text4 = "[fnt_pixel]Isso porque nossa ilha é um grande [c_red]cemitério[/c] onde as pessoas são levadas para serem enterradas.";
			text5 = "[fnt_pixel]Mas esses esqueletos vão assustar todas as pessoas e elas vão parar de nos visitar completamente. Por que você não vai ao nosso templo e resolve isso?";
			break;
		case 6: //pl
			text1 = "[fnt_pixel]Witaj! Witamy na naszej [c_purple]cienistej wyspie[/c]! Jak widzisz, na zewnątrz panuje chaos!";
			text2 = "[fnt_pixel]Ulice są pełne [shake]szkieletów[/shake]. A wszystko zaczęło się po tym, jak jeden [c_red]duży gość[/c] przemknął obok wioski.";
			text3 = "[fnt_pixel]Założę się, że to ktoś, kogo znasz, bo nasi goście zazwyczaj już nie poruszają się sami, heh heh heh.";
			text4 = "[fnt_pixel]To dlatego nasza wyspa to wielki [c_red]Cmentarz[/c], gdzie ludzie są przyprowadzani, aby ich pochować.";
			text5 = "[fnt_pixel]Ale te szkielety odstraszą wszystkich ludzi i przestaną nas odwiedzać. Może pójdziesz do naszej świątyni i zajmiesz się tym?";
			break;
		case 7: //jp
			text1 = "[fnt_pixel]こんにちは！私たちの[c_purple]シャドウアイランドへようこそ[/c]！見ての通り、外は大混乱です！";
			text2 = "[fnt_pixel]街は[shake]スケルトンで[/shake]溢れています。そして、それはすべて一[c_red]人の大きな男[/c]が村を抜けていった後から始まりました。";
			text3 = "[fnt_pixel]それはあなたが知っている人間であるに違いない、なぜなら我々の訪問客は通常、自分自身を動かすことはないからね、へへへ。";
			text4 = "[fnt_pixel]それは、我々の島が人々が埋葬されるために連れて行かれる[c_red]大きな墓地だからです[/c]。";
			text5 = "[fnt_pixel]しかし、これらのスケルトンはすべての人々を怖がらせ、彼らは私たちを訪れるのを完全に止めてしまうでしょう。なぜ、我々の寺院に行ってそれを処理しないのですか？";
			break;
		case 8: //de
			text1 = "[fnt_pixel]Hallo! Willkommen auf unserer [c_purple]Schatteninsel[/c]! Wie Sie sehen können, ist draußen Chaos! ";
			text2 = "[fnt_pixel]Die Straßen sind überlaufen mit [shake]Skeletten[/shake]. Und alles begann, nachdem ein [c_red]großer Kerl[/c] das Dorf heimlich passiert hat. ";
			text3 = "[fnt_pixel]Ich wette, es ist jemand, den Sie kennen, weil unsere Besucher normalerweise nicht mehr von alleine herumlaufen, heh heh heh. ";
			text4 = "[fnt_pixel]Das liegt daran, dass unsere Insel ein großer [c_red]Friedhof[/c] ist, wohin die Menschen zum Begraben gebracht werden. ";
			text5 = "[fnt_pixel]Aber diese Skelette werden alle Leute erschrecken und sie werden aufhören, uns überhaupt noch zu besuchen. Warum gehen Sie nicht zu unserem Tempel und kümmern sich darum?";
			break;
		case 9: //fr
			text1 = "[fnt_pixel]Bonjour! Bienvenue sur notre [c_purple]île des ombres[/c]! Comme tu peux le voir, c'est le chaos dehors!";
			text2 = "[fnt_pixel]Les rues sont envahies de [shake]squelettes[/shake]. Et tout a commencé après qu'un [c_red]grand gaillard[/c] se soit faufilé dans le village.";
			text3 = "[fnt_pixel]Je parie que c'est quelqu'un que tu connais, car nos visiteurs ne bougent généralement plus seuls, heh heh heh.";
			text4 = "[fnt_pixel]C'est parce que notre île est un grand [c_red]cimetière[/c] où les gens sont amenés pour être enterrés.";
			text5 = "[fnt_pixel]Mais ces squelettes vont effrayer toutes les personnes et elles arrêteront de nous rendre visite complètement. Pourquoi n'irais-tu pas dans notre temple pour t'en occuper?";
			break;
		case 10: //it
			text1 = "[fnt_pixel]Ciao! Benvenuto nella nostra [c_purple]isola ombra[/c]! Come puoi vedere, fuori è un caos!";
			text2 = "[fnt_pixel]Le strade sono invase da [shake]scheletri[/shake]. E tutto è iniziato dopo che un [c_red]grosso tizio[/c] si è intrufolato oltre il villaggio.";
			text3 = "[fnt_pixel]Scommetto che è qualcuno che conosci, perché i nostri visitatori di solito non si muovono più da soli, ehehehe.";
			text4 = "[fnt_pixel]Questo perché la nostra isola è un grande [c_red]cimitero[/c] dove le persone vengono portate per essere sepolte.";
			text5 = "[fnt_pixel]Ma questi scheletri spaventeranno tutte le persone e smetteranno di visitarci del tutto. Perché non vai al nostro tempio e te ne occupi?";
			break;
    }   
        
}

function scr_txt_shadow_chief2(){
    switch(global.language){
        case 0:
            #region English
            text1 = "[fnt_pixel]Finally, my boys can get back to their posts. And it's all thanks to you!";
            text2 = "[fnt_pixel]Now people from neighboring islands will be able to come here as before without fear.";
            text3 = "[fnt_pixel]You've done a great job! Here's a [c_yellow]little gift[/c] from all of us.";
            #endregion
            break;
		case 1: //rus
			text1 = "[fnt_pixel_ru]Наконец-то мои ребята смогут вернуться на свои посты. И все благодаря тебе!";
			text2 = "[fnt_pixel_ru]Теперь люди с соседних островов смогут как и прежде приезжать сюда и ничего не бояться.";
			text3 = "[fnt_pixel_ru]Ты отлично поработал! Вот [c_yellow]небольшой подарок[/c] от всех нас.";
			break;
		case 2: //es
			text1 = "[fnt_pixel]Finalmente, mis chicos pueden volver a sus puestos. ¡Y todo gracias a ti!";
			text2 = "[fnt_pixel]Ahora la gente de las islas vecinas podrá venir aquí como antes sin miedo.";
			text3 = "[fnt_pixel]¡Has hecho un gran trabajo! Aquí tienes un [c_yellow]pequeño regalo[/c] de todos nosotros.";
			break;
		case 3: //cn
			text1 = "[fnt_pixel]终于，我的孩子们可以回到他们的岗位上了。这全都多亏你！";
			text2 = "[fnt_pixel]现在，邻近岛屿的人们将可以像以前一样无惧地来到这里。";
			text3 = "[fnt_pixel]你做得很好！这是我们大家给你的一[c_yellow]份小礼物[/c]。";
			break;
		case 4: //pt
			text1 = "[fnt_pixel]Finalmente, meus rapazes podem voltar para os seus postos. E tudo graças a você!";
			text2 = "[fnt_pixel]Agora, as pessoas das ilhas vizinhas serão capazes de vir aqui como antes sem medo.";
			text3 = "[fnt_pixel]Você fez um ótimo trabalho! Aqui está um [c_yellow]pequeno[/c] presente de todos nós.";
			break;
		case 5: //br
			text1 = "[fnt_pixel]Finalmente, meus garotos podem voltar para seus postos. E tudo graças a você!";
			text2 = "[fnt_pixel]Agora as pessoas das ilhas vizinhas poderão vir aqui como antes sem medo.";
			text3 = "[fnt_pixel]Você fez um ótimo trabalho! Aqui está um [c_yellow]pequeno presente[/c] de todos nós.";
			break;
		case 6: //pl
			text1 = "[fnt_pixel]Wreszcie moi chłopcy mogą wrócić na swoje stanowiska. I to wszystko dzięki tobie!";
			text2 = "[fnt_pixel]Teraz ludzie z sąsiednich wysp będą mogli tu przyjeżdżać jak wcześniej, bez strachu.";
			text3 = "[fnt_pixel]Wykonałeś doskonałą robotę! Oto [c_yellow]mały prezent[/c] od nas wszystkich.";
			break;
		case 7: //jp
			text1 = "[fnt_pixel]ついに、私の部下たちが彼らのポストに戻ることができます。そして、それはすべてあなたのおかげです！";
			text2 = "[fnt_pixel]これで、近隣の島々からの人々が以前のように恐れることなくここに来ることができます。";
			text3 = "[fnt_pixel]素晴らしい仕事をしてくれました！[c_yellow]これが私たち全員からの小さなプレゼントです[/c]。";
			break;
		case 8: //de
			text1 = "[fnt_pixel]Endlich können meine Jungs zu ihren Posten zurückkehren. Und das ist alles dank dir! ";
			text2 = "[fnt_pixel]Jetzt können Menschen von benachbarten Inseln wieder ohne Angst hierher kommen. ";
			text3 = "[fnt_pixel]Du hast einen tollen Job gemacht! Hier ist ein [c_yellow]kleines Geschenk[/c] von uns allen.";
			break;
		case 9: //fr
			text1 = "[fnt_pixel]Enfin, mes garçons peuvent retourner à leurs postes. Et c'est grâce à toi!";
			text2 = "[fnt_pixel]Maintenant, les gens des îles voisines pourront venir ici comme avant sans crainte.";
			text3 = "[fnt_pixel]Tu as fait un excellent travail! Voici un [c_yellow]petit cadeau[/c] de nous tous.";
			break;
		case 10: //it
			text1 = "[fnt_pixel]Finalmente, i miei ragazzi possono tornare ai loro posti. E tutto grazie a te!";
			text2 = "[fnt_pixel]Ora le persone delle isole vicine potranno venire qui come prima senza paura.";
			text3 = "[fnt_pixel]Hai fatto un ottimo lavoro! Ecco un [c_yellow]piccolo regalo[/c] da parte di tutti noi.";
			break;
    }       
}