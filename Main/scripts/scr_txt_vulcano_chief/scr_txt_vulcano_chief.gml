function scr_txt_vulcano_chief1(){
    switch(global.language){
        case 0:
            #region English
            text1 = "[fnt_pixel]Welcome to our [c_maroon]vulcano island[/c]!";
            text2 = "[fnt_pixel]Our island is rich in [c_red]crystals[/c], which we use as currency in our archipelago.";
            text3 = "[fnt_pixel]Unfortunately something went wrong, the volcanoes woke up and now our village is filled with smog.";
            text4 = "[fnt_pixel]This is probably due to a [c_red]girl[/c] who recently passed by our village.";
            text5 = "[fnt_pixel]She was on her way to our temple and it's likely to have affected our island in some way.";
            text6 = "[fnt_pixel]I see you're not affected by this smog, so maybe you can dispel it.";
            text7 = "[fnt_pixel]You should head to the temple, perhaps there you can find the source of the problem.";
            #endregion
            break;
		case 1: //rus
			text1 = "[fnt_pixel_ru]О приветствую на нашем [c_maroon]вулканическом острове[/c]!";
			text2 = "[fnt_pixel_ru]Наш остров богат [c_red]кристаллами[/c], которые мы используем в качестве валюты на нашем архипелаге.";
			text3 = "[fnt_pixel_ru]К сожалению что-то пошло не так, вулканы проснулись и теперь нашу деревню заполонил дым.";
			text4 = "[fnt_pixel_ru]Возможно это связано с [c_red]девчонкой[/c], которая недавно проходила мимо нашей деревни.";
			text5 = "[fnt_pixel_ru]Она направлялась в наш храм и скорее всего это как то повлияло на наш остров.";
			text6 = "[fnt_pixel_ru]Я посмотрю на тебя этот дым не действует, поэтому, может быть, ты сможешь развеять его?";
			text7 = "[fnt_pixel_ru]Тебе нужно направиться в храм, возможно, там ты найдешь источник проблем.";
			break;
		case 2: //es
			text1 = "[fnt_pixel]Bienvenido a nuestra [c_maroon]isla volcánica[/c]!";
			text2 = "[fnt_pixel]Nuestra isla es rica en [c_red]cristales[/c], que utilizamos como moneda en nuestro archipiélago.";
			text3 = "[fnt_pixel]Desafortunadamente, algo salió mal, los volcanes se despertaron y ahora nuestro pueblo está lleno de humo.";
			text4 = "[fnt_pixel]Probablemente esto se deba a una [c_red]chica[/c] que pasó recientemente por nuestro pueblo.";
			text5 = "[fnt_pixel]Ella estaba de camino a nuestro templo y es probable que haya afectado a nuestra isla de alguna manera.";
			text6 = "[fnt_pixel]Veo que no estás afectado por este humo, así que tal vez puedas disiparlo.";
			text7 = "[fnt_pixel]Deberías dirigirte al templo, tal vez allí puedas encontrar la fuente del problema.";
			break;
		case 3: //cn
			text1 = "[fnt_pixel]欢迎来到我[c_maroon]们的火山岛[/c]！";
			text2 = "[fnt_pixel]我们的岛屿富含[c_red]水晶[/c]，我们将其用作我们群岛的货币。";
			text3 = "[fnt_pixel]不幸的是，出了一些问题，火山醒了过来，现在我们的村庄充满了烟雾。";
			text4 = "[fnt_pixel]这可能是因为最近有个[c_red]女孩经过[/c]我们村庄的原因。";
			text5 = "[fnt_pixel]她正在去我们的寺庙的路上，很可能以某种方式影响了我们的岛屿。";
			text6 = "[fnt_pixel]我看你没有被这个烟雾所影响，所以也许你可以驱散它。";
			text7 = "[fnt_pixel]你应该去寺庙那儿，也许那儿能找到问题的源头。";
			break;
		case 4: //pt
			text1 = "[fnt_pixel]Bem-vindo à nossa [c_maroon]ilha do vulcão[/c]!";
			text2 = "[fnt_pixel]Nossa ilha é rica em [c_red]cristais[/c], que usamos como moeda em nosso arquipélago.";
			text3 = "[fnt_pixel]Infelizmente algo deu errado, os vulcões acordaram e agora nossa aldeia está cheia de fumaça.";
			text4 = "[fnt_pixel]Provavelmente isso se deve a uma [c_red]menina[/c] que passou recentemente pela nossa aldeia.";
			text5 = "[fnt_pixel]Ela estava a caminho do nosso templo e é provável que tenha afetado nossa ilha de alguma forma.";
			text6 = "[fnt_pixel]Vejo que você não é afetado por esta fumaça, então talvez você possa dissipá-la.";
			text7 = "[fnt_pixel]Você deve ir ao templo, talvez lá você possa encontrar a fonte do problema.";
			break;
		case 5: //br
			text1 = "[fnt_pixel]Bem-vindo à nossa [c_maroon]ilha vulcânica[/c]!";
			text2 = "[fnt_pixel]Nossa ilha é rica em [c_red]cristais[/c], que usamos como moeda no nosso arquipélago.";
			text3 = "[fnt_pixel]Infelizmente algo deu errado, os vulcões acordaram e agora nossa aldeia está cheia de fumaça.";
			text4 = "[fnt_pixel]Isso provavelmente se deve a uma [c_red]garota[/c] que passou recentemente pela nossa aldeia.";
			text5 = "[fnt_pixel]Ela estava a caminho do nosso templo e é provável que tenha afetado nossa ilha de alguma maneira.";
			text6 = "[fnt_pixel]Vejo que você não é afetado por esta fumaça, então talvez você possa dissolvi-la.";
			text7 = "[fnt_pixel]Você deveria ir ao templo, talvez lá você possa encontrar a fonte do problema.";
			break;
		case 6: //pl
			text1 = "[fnt_pixel]Witamy na naszej [c_maroon]wyspie wulkanicznej[/c]!";
			text2 = "[fnt_pixel]Nasza wyspa jest bogata w [c_red]kryształy[/c], które wykorzystujemy jako walutę na naszym archipelagu.";
			text3 = "[fnt_pixel]Niestety coś poszło nie tak, wulkany się obudziły i teraz nasza wioska jest pełna smogu.";
			text4 = "[fnt_pixel]Prawdopodobnie jest to związane z [c_red]dziewczyną[/c], która niedawno przechodziła obok naszej wioski.";
			text5 = "[fnt_pixel]Była ona w drodze do naszej świątyni i prawdopodobnie w jakiś sposób wpłynęła na naszą wyspę.";
			text6 = "[fnt_pixel]Widzę, że ten smog na ciebie nie wpływa, więc może jesteś w stanie go przepędzić.";
			text7 = "[fnt_pixel]Powinieneś udać się do świątyni, być może tam znajdziesz źródło problemu.";
			break;
		case 7: //jp
			text1 = "[fnt_pixel][c_maroon]私たちの火山島[/c]へようこそ！";
			text2 = "[fnt_pixel]私たちの島[c_red]はクリスタル[/c]が豊富で、それを私たちの群島での通貨として使用しています。";
			text3 = "[fnt_pixel]残念ながら何かが間違って、火山が目覚め、今私たちの村はスモッグでいっぱいです。";
			text4 = "[fnt_pixel]これはおそらく最近私たちの村を通り過ぎ[c_red]た少女の[/c]せいだろう。";
			text5 = "[fnt_pixel]彼女は私たちの寺院に向かっており、何らかの形で私たちの島に影響を与える可能性があります。";
			text6 = "[fnt_pixel]あなたはこのスモッグに影響を受けていないようなので、あるいはあなたはそれを追い散らすことができます。";
			text7 = "[fnt_pixel]おそらくそこで問題の元を見つけることができるでしょう。";
			break;
		case 8: //de
			text1 = "[fnt_pixel]Willkommen auf unserer [c_maroon]Vulkaninsel[/c]!";
			text2 = "[fnt_pixel]Unsere Insel ist reich an [c_red]Kristallen[/c], die wir als Währung in unserem Archipel verwenden.";
			text3 = "[fnt_pixel]Leider ist etwas schief gegangen, die Vulkane sind erwacht und nun ist unser Dorf mit Smog gefüllt.";
			text4 = "[fnt_pixel]Das liegt wahrscheinlich an einem Mädchen, das vor kurzem an unserem Dorf vorbeigekommen ist.";
			text5 = "[fnt_pixel]Sie war auf dem Weg zu unserem Tempel und hat wahrscheinlich irgendwie unsere Insel beeinflusst.";
			text6 = "[fnt_pixel]Ich sehe, du bist von diesem Smog nicht betroffen, also kannst du ihn vielleicht vertreiben.";
			text7 = "[fnt_pixel]Du solltest zum Tempel gehen, dort findest du vielleicht die Ursache des Problems.";
			break;
		case 9: //fr
			text1 = "[fnt_pixel]Bienvenue sur notre [c_maroon]île volcanique[/c]!";
			text2 = "[fnt_pixel]Notre île est riche en [c_red]cristaux[/c], que nous utilisons comme monnaie dans notre archipel.";
			text3 = "[fnt_pixel]Malheureusement, quelque chose a mal tourné, les volcans se sont réveillés et maintenant notre village est rempli de smog.";
			text4 = "[fnt_pixel]Cela est probablement dû à une [c_red]fille[/c] qui est passée récemment par notre village.";
			text5 = "[fnt_pixel]Elle se dirigeait vers notre temple et a probablement affecté notre île d'une manière ou d'une autre.";
			text6 = "[fnt_pixel]Je vois que tu n'es pas affecté par ce smog, alors peut-être que tu peux le dissiper.";
			text7 = "[fnt_pixel]Tu devrais te diriger vers le temple, peut-être là tu peux trouver la source du problème.";
			break;
		case 10: //it
			text1 = "[fnt_pixel]Benvenuto sulla nostra [c_maroon]isola vulcanica[/c]!";
			text2 = "[fnt_pixel]La nostra isola è ricca di [c_red]cristalli[/c], che usiamo come valuta nel nostro arcipelago.";
			text3 = "[fnt_pixel]Purtroppo qualcosa è andato storto, i vulcani si sono svegliati e ora il nostro villaggio è pieno di smog.";
			text4 = "[fnt_pixel]Probabilmente a causa di una [c_red]ragazza[/c] che di recente è passata per il nostro villaggio.";
			text5 = "[fnt_pixel]Era diretta al nostro tempio ed è probabile che abbia influenzato in qualche modo la nostra isola.";
			text6 = "[fnt_pixel]Vedo che tu non sei affetto da questo smog, quindi forse potresti dissolverlo.";
			text7 = "[fnt_pixel]Dovresti andare al tempio, forse lì potrai trovare la fonte del problema.";
			break;
    }
}

function scr_txt_vulcano_chief2(){
    switch(global.language){
        case 0:
            #region English
            text1 = "[fnt_pixel]The smog is gone...that was your effort, right? Good job!";
            text2 = "[fnt_pixel]Somehow I was sure you could do it. Now our people can go back to mining the crystals.";
            text3 = "[fnt_pixel]Here's a little gift for your help.";
            #endregion
            break;
		case 1: //rus
			text1 = "[fnt_pixel_ru]Дым исчез...это же ты постарался, так? Отличная работа! ";
			text2 = "[fnt_pixel_ru]Я почему то был уверен что ты справишься. Теперь наши люди могут отправиться обратно добывать кристаллы.";
			text3 = "[fnt_pixel_ru]Вот небольшой подарок за помощь.";
			break;
		case 2: //es
			text1 = "[fnt_pixel]El humo se ha ido... eso fue tu esfuerzo, ¿verdad? ¡Buen trabajo!";
			text2 = "[fnt_pixel]De alguna manera estaba seguro de que podías hacerlo. Ahora nuestra gente puede volver a extraer los cristales.";
			text3 = "[fnt_pixel]Aquí tienes un pequeño regalo por tu ayuda.";
			break;
		case 3: //cn
			text1 = "[fnt_pixel]烟雾消失了...这是你的努力成果，对吧？做得好！";
			text2 = "[fnt_pixel]不知为何，我很确信你能做到。现在，我们的人可以回去采掘水晶了。";
			text3 = "[fnt_pixel]这是对你的帮助的小小回馈。";
			break;
		case 4: //pt
			text1 = "[fnt_pixel]A fumaça se foi... isso foi seu esforço, certo? Bom trabalho!";
			text2 = "[fnt_pixel]De alguma forma eu tinha certeza que você poderia fazer isso. Agora nosso povo pode voltar a minerar os cristais.";
			text3 = "[fnt_pixel]Aqui está um pequeno presente pela sua ajuda.";
			break;
		case 5: //br
			text1 = "[fnt_pixel]A fumaça se foi... isso foi mérito seu, certo? Bom trabalho!";
			text2 = "[fnt_pixel]De alguma forma, eu tinha certeza que você conseguiria. Agora o nosso povo pode voltar a minerar os cristais.";
			text3 = "[fnt_pixel]Aqui está um pequeno presente pela sua ajuda.";
			break;
		case 6: //pl
			text1 = "[fnt_pixel]Smog zniknął... to twoja zasługa, prawda? Dobra robota!";
			text2 = "[fnt_pixel]Jakoś byłem pewien, że dasz radę. Teraz nasi ludzie mogą wrócić do wydobywania kryształów.";
			text3 = "[fnt_pixel]Oto mały podarunek za twoją pomoc.";
			break;
		case 7: //jp
			text1 = "[fnt_pixel]スモッグがなくなりました...それはあなたの努力でしたよね？良い仕事をしました！";
			text2 = "[fnt_pixel]なぜか、あなたならそれができると確信していました。これで我々の人々はクリスタルの採掘に戻ることができます。";
			text3 = "[fnt_pixel]あなたの手助けに対する小さなプレゼントです。";
			break;
		case 8: //de
			text1 = "[fnt_pixel]Der Smog ist weg... das war dein Verdienst, nicht wahr? Gute Arbeit!";
			text2 = "[fnt_pixel]Irgendwie war ich mir sicher, dass du das schaffen kannst. Jetzt können unsere Leute zurückkehren und die Kristalle abbauen.";
			text3 = "[fnt_pixel]Hier ist ein kleines Geschenk für deine Hilfe.";
			break;
		case 9: //fr
			text1 = "[fnt_pixel]Le smog est parti... c'était ton effort, n'est-ce pas? Bon travail!";
			text2 = "[fnt_pixel]D'une manière ou d'une autre, j'étais sûr que tu pourrais le faire. Maintenant, notre peuple peut retourner à l'exploitation des cristaux.";
			text3 = "[fnt_pixel]Voici un petit cadeau pour votre aide.";
			break;
		case 10: //it
			text1 = "[fnt_pixel]Lo smog è sparito...era il tuo sforzo, vero? Buon lavoro!";
			text2 = "[fnt_pixel]In qualche modo ero sicuro che tu potessi farcela. Ora il nostro popolo può tornare a estrarre i cristalli.";
			text3 = "[fnt_pixel]Ecco un piccolo regalo per il tuo aiuto.";
			break;
    }
}
