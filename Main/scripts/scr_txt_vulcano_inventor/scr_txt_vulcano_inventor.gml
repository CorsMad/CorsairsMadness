function scr_txt_vulcano_inventor1(){
    switch(global.language){
        case 0:
            #region English
            text1 = "[fnt_pixel]Hey, Sam! Look what I got, a [shake]boomerang[/shake]!";
            text2 = "[fnt_pixel]It flies itself back to you after you throw it.";
            text3 = "[fnt_pixel]You want to test one of these? We can't do it here because of the smoke, but I just built another obstacle course nearby.";
            text4 = "[fnt_pixel]Shall we go there?";
            #endregion
            break;
		case 1:	//rus
			text1 = "[fnt_pixel_ru]Привет, Сэм! Посмотри что у меня есть, [shake]бумеранг[/shake]!";
			text2 = "[fnt_pixel_ru]Он сам прилетает к тебе назад, после того как ты кинул его.";
			text3 = "[fnt_pixel_ru]Хочешь протестировать такой же? Тут мы не сможем это сделать из-за дыма, но я как раз построил еще одну полосу препятствий недалеко.";
			text4 = "[fnt_pixel_ru]Отправимся туда?";
			break;
		case 2:	//es
			text1 = "[fnt_pixel]¡Hola, Sam! Mira lo que tengo, un [shake]bumerán[/shake]!";
			text2 = "[fnt_pixel]Vuela de vuelta a ti después de que lo lances.";
			text3 = "[fnt_pixel]¿Quieres probar uno de estos? No podemos hacerlo aquí por el humo, pero acabo de construir otro circuito de obstáculos cerca.";
			text4 = "[fnt_pixel]¿Vamos allí?";
			break;
		case 3:	//cn
			text1 = "[fnt_pixel]嘿，Sam！看看我得到了什么，[shake]一个回力标[/shake]！";
			text2 = "[fnt_pixel]你扔出去后它会自己飞回来。";
			text3 = "[fnt_pixel]你想测试一下这个吗？我们不能在这里进行测试，因为有烟，但我刚在附近建了另一个障碍赛道。";
			text4 = "[fnt_pixel]我们去那里吗？";
			break;
		case 4:	//pt
			text1 = "[fnt_pixel]Ei, Sam! Olha o que eu tenho, um [shake]bumerangue[/shake]!";
			text2 = "[fnt_pixel]Ele volta para você depois que você o joga.";
			text3 = "[fnt_pixel]Você quer testar um desses? Não podemos fazer isso aqui por causa da fumaça, mas acabei de construir outro percurso de obstáculos por perto.";
			text4 = "[fnt_pixel]Vamos para lá?";
			break;
		case 5:	//br
			text1 = "[fnt_pixel]Ei, Sam! Olha o que eu consegui, um [shake]bumerangue[/shake]!";
			text2 = "[fnt_pixel]Ele volta para você depois que você o joga.";
			text3 = "[fnt_pixel]Você quer testar um desses? Não podemos fazer isso aqui por causa da fumaça, mas acabei de construir outro percurso de obstáculos nas proximidades.";
			text4 = "[fnt_pixel]Vamos para lá?";
			break;
		case 6:	//pl
			text1 = "[fnt_pixel]Hej, Sam! Spójrz co mam, [shake]bumerang[/shake]!";
			text2 = "[fnt_pixel]Sam wraca do ciebie po rzuceniu.";
			text3 = "[fnt_pixel]Chcesz przetestować jeden z nich? Nie możemy tego zrobić tutaj z powodu dymu, ale właśnie zbudowałem kolejny tor przeszkód nieopodal.";
			text4 = "[fnt_pixel]Czy idziemy tam?";
			break;
		case 7:	//jp
			text1 = "[fnt_pixel]やあ、サム！何を手に入れたか見て、[shake]ブーメランだ[/shake]！";
			text2 = "[fnt_pixel]それを投げると自分自身が帰ってきます。";
			text3 = "[fnt_pixel]これを試してみたい？煙のせいでここではできませんが、近くに別の障害物コースを作りました。";
			text4 = "[fnt_pixel]そこへ行きましょうか？";
			break;
		case 8:	//de
			text1 = "[fnt_pixel]Hey, Sam! Sieh mal, was ich habe, einen [shake]Bumerang[/shake]!";
			text2 = "[fnt_pixel]Er fliegt von selbst zu dir zurück, nachdem du ihn geworfen hast.";
			text3 = "[fnt_pixel]Willst du einen davon ausprobieren? Wir können es hier wegen dem Rauch nicht tun, aber ich habe gerade einen weiteren Hindernisparcours in der Nähe gebaut.";
			text4 = "[fnt_pixel]Sollen wir dorthin gehen?";
			break;
		case 9:	//fr
			text1 = "[fnt_pixel]Hey, Sam! Regarde ce que j'ai, un [shake]boomerang[/shake]!";
			text2 = "[fnt_pixel]Il revient à toi tout seul après que tu l'aies jeté.";
			text3 = "[fnt_pixel]Tu veux tester l'un de ces? On ne peut pas le faire ici à cause de la fumée, mais je viens de construire un autre parcours d'obstacles à proximité.";
			text4 = "[fnt_pixel]On y va?";
			break;
		case 10: //it
			text1 = "[fnt_pixel]Ehi, Sam! Guarda cosa ho, un [shake]boomerang[/shake]!";
			text2 = "[fnt_pixel]Torna da solo a te dopo che lo lanci.";
			text3 = "[fnt_pixel]Vuoi testare uno di questi? Non possiamo farlo qui a causa del fumo, ma ho appena costruito un altro percorso ad ostacoli nelle vicinanze.";
			text4 = "[fnt_pixel]Andiamo lì?";
			break;
    }
}

function scr_txt_vulcano_inventor2(){
