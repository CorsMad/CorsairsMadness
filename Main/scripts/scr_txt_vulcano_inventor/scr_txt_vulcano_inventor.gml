			break;
		case 2: //es
			text1 = "[fnt_pixel]Presiona el botón de [c_red]habilidad especial[/c] y liberarás el bumerán. ¡Y después de un rato, volverá!";
			text2 = "[fnt_pixel]¡Además de poder cambiar de gatillo, también puede atraer algunos objetos!";
			text3 = "[fnt_pixel]¡Pero ten cuidado, algunos enemigos también se sentirán atraídos!";
			break;
		case 3: //cn
			text1 = "[fnt_pixel][c_red]按下特殊能力按钮[/c]，你就会释放回力标。过了一会儿，它就会回来！";
			text2 = "[fnt_pixel]除了能够切换触发器，它还可以吸引一些物体！";
			text3 = "[fnt_pixel]但是要小心，一些敌人也会被吸引过来！";
			break;
		case 4: //pt
			text1 = "[fnt_pixel]Pressione o botão de [c_red]habilidade especial[/c] e você soltará o bumerangue. E depois de um tempo, ele voltará!";
			text2 = "[fnt_pixel]Além de ser capaz de alternar gatilhos, também pode atrair alguns objetos!";
			text3 = "[fnt_pixel]Mas cuidado, alguns inimigos serão atraídos também!";
			break;
		case 5: //br
			text1 = "[fnt_pixel]Pressione o botão de [c_red]habilidade especial[/c] e você lançará o bumerangue. E após um tempo, ele voltará!";
			text2 = "[fnt_pixel]Além de poder alternar acionadores, ele também pode atrair alguns objetos!";
			text3 = "[fnt_pixel]Mas tenha cuidado, alguns inimigos também serão atraídos!";
			break;
		case 6: //pl
			text1 = "[fnt_pixel]Naciśnij przycisk [c_red]specjalnej umiejętności[/c], a wypuścisz bumerang. A po chwili wróci!";
			text2 = "[fnt_pixel]Oprócz możliwości przełączania spustów, może także przyciągać niektóre przedmioty!";
			text3 = "[fnt_pixel]Ale uważaj, niektórzy przeciwnicy też będą przyciągani!";
			break;
		case 7: //jp
			text1 = "[fnt_pixel][c_red]特別な能力ボタンを押すと[/c]、ブーメランを放します。そしてしばらくすると、それは戻ってきます！";
			text2 = "[fnt_pixel]トリガーを切り替えることができるだけでなく、一部の物体も引き寄せることができます！";
			text3 = "[fnt_pixel]しかし注意してください、一部の敵も引き寄せられます！";
			break;
		case 8: //de
			text1 = "[fnt_pixel]Drücke die Taste für die [c_red]Spezialfähigkeit[/c] und du lässt den Bumerang los. Und nach kurzer Zeit kommt er wieder zurück!";
			text2 = "[fnt_pixel]Neben dem Wechseln der Auslöser kann er auch einige Objekte anziehen!";
			text3 = "[fnt_pixel]Aber sei vorsichtig, einige Feinde werden auch angezogen!";
			break;
		case 9: //fr
			text1 = "[fnt_pixel]Appuyez sur le bouton de [c_red]capacité spéciale[/c] et vous libérerez le boomerang. Et après un moment, il reviendra!";
			text2 = "[fnt_pixel]En plus d'être capable de changer de déclencheurs, il peut également attirer certains objets!";
			text3 = "[fnt_pixel]Mais soyez prudent, certains ennemis seront attirés aussi!";
			break;
		case 10: //it
			text1 = "[fnt_pixel]Premi il pulsante [c_red]dell'abilità[/c] speciale e rilascerai il boomerang. E dopo un po', tornerà!";
			text2 = "[fnt_pixel]Oltre a poter scambiare i grilletti, può anche attrarre alcuni oggetti!";
			text3 = "[fnt_pixel]Ma stai attento, alcuni nemici saranno attratti anche loro!";
			break;
    }
}

