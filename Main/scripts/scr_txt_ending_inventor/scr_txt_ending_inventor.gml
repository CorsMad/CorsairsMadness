function scr_txt_ending_inventor1(){
    switch(global.language){
        case 0:
            #region eng
            text1 = "[fnt_pixel]Well, I'm sorry you couldn't save anyone.";
            text2 = "[fnt_pixel]That's a big problem. I can fix the ship. But can you handle it alone?";
            text3 = "[fnt_pixel]I highly doubt it.";
            text4 = "[fnt_pixel]I suggest you stay with us and help guard the temples.";
            text5 = "[fnt_pixel]Maybe we can invent a couple more cool devices.";
            #endregion
            break;    
		case 1: //rus
			text1 = "[fnt_pixel_ru]Что ж, жаль что тебе никого не удалось спасти.";
			text2 = "[fnt_pixel_ru]Это большая проблема. Починить корабль я, конечно, смогу. Но сможешь ли ты один отправиться в плавание?";
			text3 = "[fnt_pixel_ru]Сильно сомневаюсь.";
			text4 = "[fnt_pixel_ru]Я предлагаю тебе остаться с нами и помочь с охраной храмов.";
			text5 = "[fnt_pixel_ru]Может быть мы сможем изобрести еще парочку классных устройств.";
			break;
		case 2: //es
			text1 = "[fnt_pixel]Bueno, lamento que no pudieras salvar a nadie.";
			text2 = "[fnt_pixel]Eso es un gran problema. Puedo arreglar el barco. ¿Pero puedes manejarlo solo?";
			text3 = "[fnt_pixel]Lo dudo mucho.";
			text4 = "[fnt_pixel]Te sugiero que te quedes con nosotros y ayudes a proteger los templos.";
			text5 = "[fnt_pixel]Quizás podamos inventar un par de dispositivos más geniales.";
			break;
		case 3: //cn
			text1 = "[fnt_pixel]哎，我很遗憾你没能救出任何人。";
			text2 = "[fnt_pixel]这是个大问题。我可以修理船只。但是你能独自应对吗？";
			text3 = "[fnt_pixel]我非常怀疑。";
			text4 = "[fnt_pixel]我建议你留在我们这里，帮助守护神庙。";
			text5 = "[fnt_pixel]也许我们可以再发明一些酷炫的设备。";
			break;
		case 4: //pt
			text1 = "[fnt_pixel]Bem, lamento que você não tenha conseguido salvar ninguém.";
			text2 = "[fnt_pixel]Isso é um grande problema. Eu posso consertar o navio. Mas você consegue lidar com isso sozinho?";
			text3 = "[fnt_pixel]Eu duvido muito.";
			text4 = "[fnt_pixel]Eu sugiro que você fique conosco e ajude a guardar os templos.";
			text5 = "[fnt_pixel]Talvez possamos inventar mais alguns dispositivos legais.";
			break;
		case 5: //br
			text1 = "[fnt_pixel]Bem, lamento que você não tenha conseguido salvar ninguém. ";
			text2 = "[fnt_pixel]Isso é um grande problema. Eu posso consertar o navio. Mas você consegue lidar com isso sozinho? ";
			text3 = "[fnt_pixel]Duvido muito. ";
			text4 = "[fnt_pixel]Sugiro que você fique conosco e ajude a guardar os templos. ";
			text5 = "[fnt_pixel]Talvez possamos inventar mais alguns dispositivos legais.";
			break;
		case 6: //pl
			text1 = "[fnt_pixel]Cóż, przykro mi, że nie udało ci się nikogo uratować.";
			text2 = "[fnt_pixel]To duży problem. Mogę naprawić statek. Ale dasz radę sam?";
			text3 = "[fnt_pixel]Bardzo w to wątpię.";
			text4 = "[fnt_pixel]Sugeruję, żebyś został z nami i pomógł strzec świątynie.";
			text5 = "[fnt_pixel]Może wymyślimy jeszcze kilka fajnych urządzeń.";
			break;
		case 7: //jp
			text1 = "[fnt_pixel]あ、誰も救えなかったことを残念に思います。";
			text2 = "[fnt_pixel]それは大問題です。私は船を修理することができます。でもあなた一人で取り扱うことができますか？";
			text3 = "[fnt_pixel]それは大いに疑わしい。";
			text4 = "[fnt_pixel]私たちと一緒に残って神殿の警備を手伝うことを提案します。";
			text5 = "[fnt_pixel]もしかしたら、もう2、3個、カッコいい機器を発明できるかもしれませんね。";
			break;
		case 8: //de
			text1 = "[fnt_pixel]Nun, es tut mir leid, dass du niemanden retten konntest.";
			text2 = "[fnt_pixel]Das ist ein großes Problem. Ich kann das Schiff reparieren. Aber kannst du das alleine bewältigen?";
			text3 = "[fnt_pixel]Das bezweifle ich stark.";
			text4 = "[fnt_pixel]Ich schlage vor, du bleibst bei uns und hilfst, die Tempel zu bewachen.";
			text5 = "[fnt_pixel]Vielleicht können wir ein paar weitere coole Geräte erfinden.";
			break;
		case 9: //fr
			text1 = "[fnt_pixel]Eh bien, je suis désolé que tu n'aies pu sauver personne.";
			text2 = "[fnt_pixel]C'est un gros problème. Je peux réparer le navire. Mais peux-tu t'en sortir seul?";
			text3 = "[fnt_pixel]J'en doute fort.";
			text4 = "[fnt_pixel]Je te suggère de rester avec nous et d'aider à garder les temples.";
			text5 = "[fnt_pixel]Peut-être que nous pouvons inventer encore quelques appareils cool.";
			break;
		case 10: //it
			text1 = "[fnt_pixel]Beh, mi dispiace che non sei riuscito a salvare nessuno.";
			text2 = "[fnt_pixel]Questo è un grosso problema. Posso riparare la nave. Ma puoi gestirla da solo?";
			text3 = "[fnt_pixel]Ne dubito molto.";
			text4 = "[fnt_pixel]Ti suggerisco di restare con noi e aiutare a guardare i templi.";
			text5 = "[fnt_pixel]Forse possiamo inventare un paio di dispositivi più freddi.";
			break;
    }
}

function scr_txt_ending_inventor2(){
