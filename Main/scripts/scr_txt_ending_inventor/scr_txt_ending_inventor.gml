            break;  
		case 1: //rus
			text1 = "[fnt_pixel_ru]В ближайшее время я починю твой корабль и вы сможете отплыть. ";
			text2 = "[fnt_pixel_ru]Хоть неполной командой вам будет сложно управиться с кораблем, но я уверен, что вы справитесь!";
			break;
		case 2: //es
			text1 = "[fnt_pixel]Pronto arreglaré tu barco y podrás navegar.";
			text2 = "[fnt_pixel]Aunque será difícil para ti manejar el barco con una tripulación incompleta, ¡pero estoy seguro de que te las arreglarás!";
			break;
		case 3: //cn
			text1 = "[fnt_pixel]很快我就会修好你的船，你就可以扬帆出海了。";
			text2 = "[fnt_pixel]虽然你在船员不全的情况下很难驾驶船只，但我相信你一定能应对的！";
			break;
		case 4: //pt
			text1 = "[fnt_pixel]Em breve eu vou consertar seu navio e você poderá navegar.";
			text2 = "[fnt_pixel]Embora seja difícil para você manusear o navio com uma tripulação incompleta, mas tenho certeza de que você vai se sair bem!";
			break;
		case 5: //br
			text1 = "[fnt_pixel]Em breve vou consertar seu navio e você poderá navegar. ";
			text2 = "[fnt_pixel]Embora seja difícil para você manejar o navio com uma tripulação incompleta, mas tenho certeza de que você vai conseguir!";
			break;
		case 6: //pl
			text1 = "[fnt_pixel]Wkrótce naprawię twój statek i będziesz mógł płynąć.";
			text2 = "[fnt_pixel]Chociaż będzie ci trudno obsługiwać statek z niekompletną załogą, ale jestem pewny, że dasz radę!";
			break;
		case 7: //jp
			text1 = "[fnt_pixel]ぐにあなたの船を修理して、あなたは航海できます。";
			text2 = "[fnt_pixel]完全ではないクルーで船を扱うのはあなたにとって困難でしょうが、でもきっと大丈夫でしょう！";
			break;
		case 8: //de
			text1 = "[fnt_pixel]Bald werde ich dein Schiff reparieren und du kannst segeln.";
			text2 = "[fnt_pixel]Obwohl es für dich schwierig sein wird, das Schiff mit einer unvollständigen Crew zu handhaben, aber ich bin sicher, du wirst es schaffen!";
			break;
		case 9: //fr
			text1 = "[fnt_pixel]Bientôt, je réparerai votre navire et vous pourrez naviguer.";
			text2 = "[fnt_pixel]Bien qu'il vous sera difficile de gérer le navire avec un équipage incomplet, mais je suis sûr que vous y arriverez!";
			break;
		case 10: //it
			text1 = "[fnt_pixel]Presto riparerò la tua nave e potrai navigare.";
			text2 = "[fnt_pixel]Anche se ti sarà difficile gestire la nave con un equipaggio incompleto, ma sono sicuro che te la caverai!";
			break;
    }
}

