function scr_txt_jungle_npc5(){
    switch(global.language){
        case 0:
            #region eng
            text0 = "[fnt_pixel]Recently, while on patrol, I came across a suspicious wall. I've been here a long time and I've seen a lot of suspicious walls.";
            text1 = "[fnt_pixel]But this one was the most suspicious of them all. I tried to climb up, but it didn't work.";
