			break;
    }
}
    
function scr_txt_jungle_npc2(){
    switch(global.language){
        case 0:
            #region eng
            text1 = "[fnt_pixel]Recently, while on patrol, I came across a suspicious wall.";
            text2 = "[fnt_pixel]I've been here a long time and I've seen a lot of suspicious walls. But this one was the most suspicious of them all.";
            text3 = "[fnt_pixel]I tried to climb up, but I couldn't do it.";
            text4 = "[fnt_pixel]The damn [c_red]THREE weeds[/c] were spitting seeds and trying to throw me off, so I gave up and went on.";
            text5 = "[fnt_pixel]I wonder what's at the top of that wall?";
            text6 = "[fnt_pixel]And in general, turn your head more often to look for vulnerable spots on walls.You never know what you might find up there.";
            #endregion
            break;
		case 1:
			#region ru
			text1 = "[fnt_pixel_ru]Недавно во время патрулирования я набрел на подозрительную стену.";
			text2 = "[fnt_pixel_ru]Я тут уже давно и подозрительных стен видел огромное количество. Но эта была самой подозрительной из всех.";
			text3 = "[fnt_pixel_ru]Я попытался забраться наверх, но у меня ничего не получилось.";
			text4 = "[fnt_pixel_ru]Еще и чертовы [c_red]ТРИ сорняка[/c] плевались косточками и пытались скинуть меня, поэтому я бросил пытаться и пошел дальше.";
			text5 = "[fnt_pixel_ru]Интересно, что там на самом верху этой стены?";
			text6 = "[fnt_pixel_ru]И вообще чаще крути головой в поисках уязвимых мест на стенах. Никогда не знаешь что можно там найти.";
			#endregion
			break;
		case 2:
			#region sp
			text1 = "[fnt_pixel]Recientemente, mientras patrullaba, me encontré con una pared sospechosa.";
			text2 = "[fnt_pixel]He estado aquí mucho tiempo y he visto muchas paredes sospechosas. Pero esta era la más sospechosa de todas.";
			text3 = "[fnt_pixel]Intenté trepar, pero no pude hacerlo.";
			text4 = "[fnt_pixel]Las malditas [c_red]tres malas hierbas[/c] estaban escupiendo semillas e intentaban tirarme, así que renuncié y seguí adelante.";
			text5 = "[fnt_pixel]¿Me pregunto qué hay en la cima de esa pared?";
			text6 = "[fnt_pixel]Y en general, gira tu cabeza con más frecuencia para buscar puntos vulnerables en las paredes. Nunca sabes lo que puedes encontrar allí arriba.";
			#endregion
			break;
		case 3:
			#region cn
			text1 = "[fnt_pixel]最近，在巡逻时，我发现了一堵可疑的墙。";
			text2 = "[fnt_pixel]我在这儿已经很长时间了，我看到过很多可疑的墙。但这一堵是最可疑的。";
			text3 = "[fnt_pixel]我试图爬上去，但我做不到。";
			text4 = "[fnt_pixel]那该死的[c_red]三棵野草[/c]正在吐种子，试图把我扔下去，所以我放弃了，继续走。";
			text5 = "[fnt_pixel]我想知道那堵墙的顶部是什么？";
			text6 = "[fnt_pixel]总的来说，要多转转头，寻找墙壁上的薄弱点。你永远不知道你可能会在那里发现什么。";
			#endregion
			break;
		case 4:
			#region pt
			text1 = "[fnt_pixel]Recentemente, durante uma patrulha, me deparei com uma parede suspeita.";
			text2 = "[fnt_pixel]Estou aqui há muito tempo e já vi muitas paredes suspeitas. Mas essa foi a mais suspeita de todas.";
			text3 = "[fnt_pixel]Tentei escalar, mas não consegui.";
			text4 = "[fnt_pixel]As malditas [c_red]três ervas daninhas[/c] estavam cuspindo sementes e tentando me derrubar, então desisti e continuei.";
			text5 = "[fnt_pixel]O que será que tem no topo dessa parede?";
			text6 = "[fnt_pixel]E, em geral, vire a cabeça mais vezes para procurar pontos vulneráveis nas paredes. Nunca se sabe o que você pode encontrar lá em cima.";
			#endregion
			break;
		case 5:
			#region br
			text1 = "[fnt_pixel]Recentemente, enquanto estava de patrulha, me deparei com uma parede suspeita.";
			text2 = "[fnt_pixel]Eu estive aqui muito tempo e já vi muitas paredes suspeitas. Mas esta era a mais suspeita de todas.";
			text3 = "[fnt_pixel]Eu tentei subir, mas não consegui.";
			text4 = "[fnt_pixel]As malditas [c_red]três ervas daninhas[/c] estavam cuspindo sementes e tentando me derrubar, então desisti e segui em frente.";
			text5 = "[fnt_pixel]Eu me pergunto o que está no topo daquela parede?";
			text6 = "[fnt_pixel]E em geral, vire mais a cabeça para procurar pontos vulneráveis nas paredes. Você nunca sabe o que pode encontrar lá em cima.";
			#endregion
			break;
		case 6:
			#region pl
			text1 = "[fnt_pixel]Ostatnio, podczas patrolu, natknąłem się na podejrzany mur. ";
			text2 = "[fnt_pixel]Jestem tu już długi czas i widziałem wiele podejrzanych murów. Ale ten był najbardziej podejrzany ze wszystkich.";
			text3 = "[fnt_pixel]Próbowałem się wspiąć, ale nie dałem rady.";
			text4 = "[fnt_pixel]Cholerny [c_red]trzytrawiaste plewy[/c] pluły nasionami i próbowały mnie zrzucić, więc dałem za wygraną i poszedłem dalej.";
			text5 = "[fnt_pixel]Zastanawiam się, co jest na górze tego muru?";
			text6 = "[fnt_pixel]I ogólnie rzecz biorąc, częściej odwracaj głowę, aby szukać słabych punktów na ścianach. Nie wiesz nigdy, co możesz tam znaleźć.";
			#endregion
			break;
		case 7:
			#region jp
			text1 = "[fnt_pixel]最近、パトロール中に怪しい壁に遭遇しました.";
			text2 = "[fnt_pixel]私はここに長い間いますし、たくさんの怪しい壁を見てきました. しかし、これが一番怪しいものでした.";
			text3 = "[fnt_pixel]登ろうとしましたが、できませんでした.";
			text4 = "[fnt_pixel]このクソ[c_red]三つの雑草[/c]が種を吹き出して私を投げようとしていたので、諦めて先に進みました.";
			text5 = "[fnt_pixel]あの壁の上に何があるのだろう?";
			text6 = "[fnt_pixel]そして全般的に、壁の脆弱な場所を探すために頭をもっと頻繁に回すようにしてください.あそこで何を見つけるか分かりませんよ.";
			#endregion
			break;
		case 8:
			#region de
			text1 = "[fnt_pixel]Vor kurzem, während einer Patrouille, stieß ich auf eine verdächtige Wand.";
			text2 = "[fnt_pixel]Ich bin schon lange hier und habe viele verdächtige Wände gesehen. Aber diese war die verdächtigste von allen.";
			text3 = "[fnt_pixel]Ich versuchte hinaufzuklettern, aber ich konnte es nicht.";
			text4 = "[fnt_pixel]Die verfluchten [c_red]drei Unkräuter[/c] spuckten Samen und versuchten mich abzuwerfen, so dass ich aufgab und weiterging.";
			text5 = "[fnt_pixel]Ich frage mich, was wohl oben auf dieser Wand ist?";
			text6 = "[fnt_pixel]Und im Allgemeinen, drehen Sie öfter den Kopf, um nach verwundbaren Stellen an Wänden zu suchen. Man weiß nie, was man dort oben finden könnte.";
			#endregion
			break;
		case 9:
			#region fr
			text1 = "[fnt_pixel]Récemment, en patrouille, je suis tombé sur un mur suspect.";
			text2 = "[fnt_pixel]Je suis là depuis longtemps et j'ai vu beaucoup de murs suspects. Mais celui-ci était le plus suspect de tous.";
			text3 = "[fnt_pixel]J'ai essayé de grimper, mais je n'y suis pas arrivé.";
			text4 = "[fnt_pixel]Les damnées [c_red]trois mauvaises herbes[/c] crachaient des graines et essayaient de me faire tomber, alors j'ai abandonné et j'ai continué.";
			text5 = "[fnt_pixel]Je me demande ce qu'il y a en haut de ce mur ?";
			text6 = "[fnt_pixel]Et en général, tournez plus souvent la tête pour chercher des points vulnérables sur les murs. On ne sait jamais ce qu'on peut y trouver.";
			#endregion
			break;
		case 10:
			#region it
			text1 = "[fnt_pixel]Di recente, durante una pattuglia, mi sono imbattuto in un muro sospetto.";
			text2 = "[fnt_pixel]Sono qui da molto tempo e ho visto un sacco di muri sospetti. Ma questo era il più sospetto di tutti.";
			text3 = "[fnt_pixel]Ho provato a scalarlo, ma non ci sono riuscito.";
			text4 = "[fnt_pixel]Le maledette [c_red]tre erbacce[/c] sputavano semi e cercavano di buttarmi giù, quindi ho rinunciato e sono andato avanti.";
			text5 = "[fnt_pixel]Chissà cosa c'è in cima a quel muro?";
			text6 = "[fnt_pixel]E in generale, gira più spesso la testa per cercare punti deboli sui muri. Non si sa mai cosa si possa trovare lassù.";
			#endregion
			break;
    }
}    

